			<xs:documentation>Ligne d'adresse 4</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="adrLgn5" type="xs:string">
		<xs:annotation>
			<xs:documentation>Ligne d'adresse 5</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="adrLgn6" type="xs:string">
		<xs:annotation>
			<xs:documentation>Ligne d'adresse 6</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="demUrg">
		<xs:annotation>
			<xs:documentation>Demande urgente :OUI ou NON : information n'ayant encore aucune incidence sur le traitement</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="3"/>
				<xs:enumeration value="OUI"/>
				<xs:enumeration value="NON"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:annotation>
		<xs:documentation>éléments de IdentificationCommunication</xs:documentation>
	</xs:annotation>
	<xs:element name="numRps" type="typEntier">
