			<xs:documentation>Prénom de l'individu</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="datNaiIdv" type="typDate">
		<xs:annotation>
			<xs:documentation>Date de naissance de l'individu</xs:documentation>
		</xs:annotation>
	</xs:element>
