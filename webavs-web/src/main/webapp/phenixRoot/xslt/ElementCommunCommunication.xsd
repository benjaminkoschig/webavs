			<xs:documentation>Numéro de la réponse</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="datRpsFrt" type="typDatFrt">
		<xs:annotation>
			<xs:documentation>Date de la réponse avec le mois complet</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="datRps" type="typDate">
		<xs:annotation>
			<xs:documentation>Date de la réponse</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nomCtcCom">
		<xs:annotation>
			<xs:documentation>Nom de la personne de contact pour la réponse : Titre - Prénom - Nom de l'utilisateur</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="eMaiCtcCom">
		<xs:annotation>
			<xs:documentation>Adresse Email de la personne de contact pour la réponse</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="telCtcCom">
		<xs:annotation>
			<xs:documentation>Numéro de téléphone de la personne de contact pour la réponse</xs:documentation>
		</xs:annotation>
