			<xs:documentation>"Taxation définitive" dans tous les cas, sauf "Taxation d'office" pour les taxations d'office</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="typTaxExa" type="xs:token">
		<xs:annotation>
			<xs:documentation>Type exact de la taxation, tel que défini dans TAO</xs:documentation>
		</xs:annotation>
	</xs:element>
