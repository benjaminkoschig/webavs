			<xs:restriction base="xs:token">
				<xs:length value="3"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="nomDosFol" type="xs:string">
		<xs:annotation>
			<xs:documentation>Nom du dossier FOLDERS</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="typDocFol">
		<xs:annotation>
			<xs:documentation>Type de document FOLDERS</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:length value="3"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="nomDocFol">
		<xs:annotation>
			<xs:documentation>Nom du document FOLDERS</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:length value="40"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="datTrvFol">
		<xs:annotation>
			<xs:documentation>Date de travail FOLDERS</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:length value="8"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="nbrPgePiv" type="typEntier">
		<xs:annotation>
			<xs:documentation>Pivot pour envoi dans carton ou dans une enveloppe</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="outCom">
		<xs:annotation>
			<xs:documentation>output de la communication : PFEX : plateforme d'échange  (EAI), CTA : client tao (impression locale), IMPM : impression masse (CADEV)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="PFEX"/>
				<xs:enumeration value="CTAO"/>
				<xs:enumeration value="IMPM"/>
				<xs:enumeration value="CTA"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="frmRstCom">
		<xs:annotation>
			<xs:documentation>Format de restitution (rendition) de la communication (PAP, ELE. EML, ...) </xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:enumeration value="PAP"/>
				<xs:enumeration value="OCC"/>
				<xs:enumeration value="ACI"/>
				<xs:enumeration value="CHM"/>
				<xs:enumeration value="PDF"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="codRetTtt" type="typEntier">
		<xs:annotation>
			<xs:documentation>1 si (ICCD et (nomPrfTie == 1 ou nomPrfTie == 2)) ~ 
Code de retour du traitement de la demande, en cas de non réussite ou impossibilité de communiquer.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="lllRetTtt" type="xs:token">
		<xs:annotation>
			<xs:documentation>Message / raison de l'échec du traitement pour cet individu</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:annotation>
		<xs:documentation>éléments de identificationResponsableTaxation</xs:documentation>
	</xs:annotation>
	<xs:element name="ideResTax">
		<xs:annotation>
			<xs:documentation>Id de la personne ayant validé la taxation, zzt si la taxation a été validée automatiquement - ID2005</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:token">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="eMaiResTax" type="xs:string">
		<xs:annotation>
			<xs:documentation>eMail de la personne ayant validé la taxation</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="telResTax">
		<xs:annotation>
			<xs:documentation>Téléphone professionnel de la personne ayant validé la taxation - ID2010</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="59"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="faxResTax">
		<xs:annotation>
			<xs:documentation>Fax professionnel de la personne ayant validé la taxation - ID2011</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="tttParResTax" type="xs:string">
		<xs:annotation>
			<xs:documentation>Titre, initiales et nom de la personne ayant validé la taxation - ID2004 +' ' +ID2015+' '+ ID2002</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ntrRefResTax" type="xs:string">
		<xs:annotation>
			<xs:documentation>ID306+'/'+ UpperCase(ID2005)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:annotation>
		<xs:documentation>éléments de identificationOIDResponsable</xs:documentation>
	</xs:annotation>
	<xs:element name="numOID" type="xs:integer">
		<xs:annotation>
			<xs:documentation>Numéro de l'OID</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="locOID" type="xs:string">
		<xs:annotation>
			<xs:documentation>Localité postale de l'OID du for de gestion pour la période d'imposition communiquée - ID2013</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="adrOID">
		<xs:annotation>
			<xs:documentation>Adresse de l'OID du for de gestion pour la période d'imposition communiquée:
UpperCase(ID2016)
UpperCase(ID2006)
ID2007
ID2008
ID2009
</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:group ref="adrGrp"/>
		</xs:complexType>
	</xs:element>
	<xs:annotation>
		<xs:documentation>éléments de identificationDemande</xs:documentation>
	</xs:annotation>
	<xs:element name="numDem" type="typEntier">
		<xs:annotation>
			<xs:documentation>Numéro de la demande</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="datDem" type="typDate">
		<xs:annotation>
			<xs:documentation>Date de la demande</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="codTypCom" default="NA">
		<xs:annotation>
			<xs:documentation>Code du type de communication. Par défaut : NA. Par exemple pour l'AVS, catégorie de l'assuré.
L'une des 4 valeurs :
"PCI" = Indépendants ou exploitants
"PSA" = Sans activité lucrative
"SAL" = Salariés affiliés indépendants
"AGR" = Collaborateurs agricoles; AVS = ID1456</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="IAF"/>
				<xs:enumeration value="PCI"/>
				<xs:enumeration value="PSA"/>
				<xs:enumeration value="AGR"/>
				<xs:enumeration value="SAL"/>
				<xs:enumeration value="NA"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="codNatCom" type="xs:token">
		<xs:annotation>
			<xs:documentation>Nature de la communication Par défaut = NA, par exemple pour l'AVS = "SP", "R 1", "R 2", "R N" - AVS = ID1446 ou ID1466
</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="numAffAvs" type="xs:string">
		<xs:annotation>
			<xs:documentation>Numéro affilié AVS</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="numAvsIdv" type="xs:string">
		<xs:annotation>
			<xs:documentation>Numéro AVS de l'individu pour lequel la demande a été faite</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="prdFisCcn" type="typAnnee">
		<xs:annotation>
			<xs:documentation>ID301 Période fiscale concernée par la demande. Si renseigné dans informationDemande, ne doit pas être renseigné au niveau de l'individuConcerne et réciproquement</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="debPrdCcn" type="xs:string">
		<xs:annotation>
			<xs:documentation>Date</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="finPrdCcn" type="xs:string">
		<xs:annotation>
			<xs:documentation>Date</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:annotation>
		<xs:documentation>éléments de identificationCtb</xs:documentation>
	</xs:annotation>
	<xs:element name="numCtb" type="xs:token">
		<xs:annotation>
			<xs:documentation>Numéro de contribuable de l'individu pour lequel est fait la demande - ID306</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nomCtb" type="xs:string">
