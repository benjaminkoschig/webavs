	</xs:element>
	<xs:annotation>
		<xs:documentation>éléments de coordonneeCtb</xs:documentation>
	</xs:annotation>
	<xs:element name="numTelCtc" type="xs:string">
		<xs:annotation>
			<xs:documentation>Numéro de téléphone de contact du contribuable - ID320</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:annotation>
		<xs:documentation>éléments de identificationTiers</xs:documentation>
	</xs:annotation>
	<xs:element name="nomCtcTie">
		<xs:annotation>
			<xs:documentation>Nom de la personne de contact pour la demande</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="eMaiCtcTie">
		<xs:annotation>
			<xs:documentation>Adresse Email de la personne de contact pour la demande</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:annotation>
		<xs:documentation>éléments de identificationDemande</xs:documentation>
	</xs:annotation>
	<xs:element name="telCtcTie">
		<xs:annotation>
			<xs:documentation>Numéro de téléphone de la personne de contact pour la demande</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:annotation>
		<xs:documentation>éléments de queueDemande</xs:documentation>
	</xs:annotation>
	<xs:element name="nbrIdvDem" type="typEntier">
		<xs:annotation>
			<xs:documentation>Nombre d'individus pour lesquels une demande a été faite dans le fichier courant</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:annotation>
		<xs:documentation>éléments de individuConcerne</xs:documentation>
	</xs:annotation>
	<xs:element name="nomIdv" type="xs:string">
