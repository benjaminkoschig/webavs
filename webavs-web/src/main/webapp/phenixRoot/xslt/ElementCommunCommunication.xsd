		<xs:annotation>
			<xs:documentation>Numéro ACI de la commune de for de gestion - ID300</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="numCmnForGes" type="typEntier">
		<xs:annotation>
			<xs:documentation>Numéro OFS de la commune de for de gestion - ID300</xs:documentation>
		</xs:annotation>
	</xs:element>
