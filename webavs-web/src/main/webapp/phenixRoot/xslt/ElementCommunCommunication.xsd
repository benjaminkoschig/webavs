			<xs:documentation>Sigle de la collectivité administrative : numéro d'identification du tiers</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="20"/>
				<xs:minLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="eMaiDst" type="xs:string">
		<xs:annotation>
			<xs:documentation>EMail du destinataire</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:annotation>
		<xs:documentation>éléments de tttCom</xs:documentation>
	</xs:annotation>
	<xs:element name="nomFic" type="xs:string"/>
