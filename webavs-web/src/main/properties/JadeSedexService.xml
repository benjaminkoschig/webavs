<?xml version="1.0" encoding="ISO-8859-1"?>
<!--globaz.jade.sedex.JadeSedexService: service d'interfaçage avec SEDEX-->
<globaz.jade.sedex.JadeSedexService>
	<testDeliveryFlag>true</testDeliveryFlag>
	<!--dir.input: répertoire où arrivent les messages SEDEX destinés à cette application-->
	<!--dir.output: répertoire où sont déposés les messages SEDEX générés par cette application-->
<!-- 	<dir.input checkDelay="1m">d:\data\sedex\inbox</dir.input> -->
	<dir.input checkDelay="1m">d:\data\sedex\inbox</dir.input>
	<dir.output>d:\data\sedex\outbox</dir.output>
	<dir.output.backup>d:\data\sedex\backup</dir.output.backup>
	<!--dir.repository: emplacement du repository SEDEX, contenant notamment l'annuaire 'particiant-list'-->
	<dir.repository>d:\data\sedex\repos</dir.repository>
	<!--send.id: identifiant SEDEX de cette application-->
	<sender.id>6-150000-1</sender.id>
	<!--messageparser: nom de classe du parser des messages XML-->
	<messageparser>ch.globaz.eavs.parser.XmlParser</messageparser>
	<!-- mappings -->
