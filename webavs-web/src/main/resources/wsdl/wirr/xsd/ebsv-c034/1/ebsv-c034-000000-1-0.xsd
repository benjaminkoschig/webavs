					<xs:sequence>
						<xs:element name="MassnahmenFruehintervention" type="xs:boolean">
							<xs:annotation>
								<xs:documentation xml:lang="de">FALSE,0 = nein
TRUE,1  = ja</xs:documentation>
								<xs:documentation xml:lang="fr">FALSE,0 = non
TRUE,1  = oui</xs:documentation>
							</xs:annotation>
