<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ebsv-c020="http://www.bsv.admin.ch/xmlns/ebsv-c020/1" targetNamespace="http://www.bsv.admin.ch/xmlns/ebsv-c020/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0">
	<!-- Ce fichier est issu du fichier TRAX AI-types.xsd -->
	<xs:simpleType name="VerfuegungNummer_Type">
		<xs:annotation>
			<xs:documentation xml:lang="de">Numéro de décision à onze chiffres pour mesures individuelles</xs:documentation>
			<xs:documentation xml:lang="fr">Elfstellige Nummer der Verfügung für individuelle Massnahmen</xs:documentation>
