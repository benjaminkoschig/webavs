	<!-- Annonce cas de décès aux caisses ================================================= -->
	<xs:complexType name="RRTodesfallmeldung_Type">
		<xs:annotation>
			<xs:documentation xml:lang="fr">
                Cas de décès annoncés par la Centrale aux Caisses (code d'application: 52)
            </xs:documentation>
			<xs:documentation xml:lang="de">
                Todesfallmeldung der ZAS an die AK (Anwendungsgebiet = 52)
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="KasseZweigstelle" type="ebsv-c016:IrgendeineZweigstelle_weakType"/>
			<xs:element name="VerstorbenePerson">
				<xs:annotation>
					<xs:documentation>Indications sur le défunt</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Versichertennummer8stellig" type="ebsv-c001:Versichertennummer8Stellen_Type"/>
						<xs:element name="Namensangaben" type="ebsv-c005:NameUndVorname_Type"/>
						<xs:element name="Geburtsdatum">
							<xs:annotation>
								<xs:documentation xml:lang="de">Geburtsdatum. Alle bekannten Elemente (Jahr, Monat, Tag) müssen angegeben werden.</xs:documentation>
								<xs:documentation xml:lang="fr">Date de naissance. A indiquer avec la meilleure précision connue.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:choice>
									<xs:element name="Geburtsdatum_JJJJMMTT">
										<xs:annotation>
											<xs:documentation xml:lang="de">Das vollständige Geburtsdatum ist bekannt.
</xs:documentation>
											<xs:documentation xml:lang="fr">La date de naissance complète est connue.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:date">
												<xs:minInclusive value="1848-01-01"/>
												<xs:maxInclusive value="2048-12-31"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="Geburtsdatum_JJJJMM">
										<xs:annotation>
											<xs:documentation xml:lang="de">Monat und Jahr des Geburtsdatums sind bekannt.</xs:documentation>
											<xs:documentation>L'année et le mois de la date de naissance sont connus.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:gYearMonth">
												<xs:minInclusive value="1848-01"/>
												<xs:maxInclusive value="2048-12"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="Geburtsdatum_JJJJ">
										<xs:annotation>
											<xs:documentation xml:lang="de">Nur das Geburtsjahr ist bekannt.</xs:documentation>
											<xs:documentation xml:lang="fr">Uniquement l'année de naissance est connue.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:gYear">
												<xs:minInclusive value="1848"/>
												<xs:maxInclusive value="2048"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="Heimatort">
							<xs:annotation>
								<xs:documentation>facultatif ?</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="0"/>
									<xs:maxLength value="12"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Wohnort">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="15"/>
									<xs:minLength value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Todesdatum_MMJJ" type="ebsv-c007:AHVDatumJM_Type"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="MoeglicherweiseBetroffeneLeistungsberechtigtePerson" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>
                        Indications sur les personnes qui peuvent être l'ayant droit à la prestation
                    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="KasseZweigstelle" type="ebsv-c016:IrgendeineZweigstelle_weakType">
							<xs:annotation>
								<xs:documentation>Caisse qui paie la prestation</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Versichertennummer" type="ebsv-c001:Versichertennummer11oder13Stellen_Type"/>
						<xs:element name="Vnr1Ergaenzend" type="ebsv-c001:Versichertennummer11oder13Stellen_Type" minOccurs="0"/>
						<xs:element name="Namensangaben" type="ebsv-c005:NameUndVorname_Type"/>
						<xs:element name="Leistungsart" type="ebsv-c023:Leistungsart_alle_Revisionen_strongType">
							<xs:annotation>
								<xs:documentation/>
							</xs:annotation>
						</xs:element>
						<xs:element name="Hinweis">
							<xs:annotation>
								<xs:documentation>0 = keiner 1 = Leistung schon als Abgang gemeldet</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:unsignedByte">
									<xs:minInclusive value="0"/>
									<xs:maxInclusive value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
