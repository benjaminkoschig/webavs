<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ebsv-c015="http://www.bsv.admin.ch/xmlns/ebsv-c015/1" targetNamespace="http://www.bsv.admin.ch/xmlns/ebsv-c015/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0">
	<!-- Ce fichier est issu du fichier TRAX Montants-types.xsd -->
	<xs:simpleType name="BetragFR_Type">
		<xs:annotation>
			<xs:documentation xml:lang="de">Beitrag im AHV-Gebiet : CHF (bis 10^9 CHF)</xs:documentation>
			<xs:documentation xml:lang="fr">Montant monétaire (en CHF) dans le domaine AVS : (jusqu'à 10^9 CHF)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:pattern value="[\+\-]?\d{1,9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BetragFRRP_Type">
		<xs:annotation>
			<xs:documentation xml:lang="de">Beitrag im AHV-Gebiet : CHF.RP (bis(10^9).99 CHF)</xs:documentation>
			<xs:documentation xml:lang="fr">Montant monétaire (en CHF.CT) dans le domaine AVS : (jusqu'à 10^9.99 CHF)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:pattern value="[\+\-]?\d{1,9}\.\d{1,2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BetragFRRP12_Type">
		<xs:annotation>
			<xs:documentation xml:lang="de">12-stelligen Betrag mit Rappen : CHF.RP (bis(12^9).99 CHF)</xs:documentation>
			<xs:documentation xml:lang="fr">Montant monétaire (en CHF.CT) à douze positions plus les décimales : (jusqu'à 10^11.99 CHF)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:pattern value="([\+\-]?\d{1,11}\.\d{1,2}|\d{1,12}\.\d{1,2})"/>
		</xs:restriction>
	</xs:simpleType>
