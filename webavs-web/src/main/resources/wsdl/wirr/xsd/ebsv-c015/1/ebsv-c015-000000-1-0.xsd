	<xs:simpleType name="BetragFR4_Type">
		<xs:annotation>
			<xs:documentation>4-stelliger Betrag CHF</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="9999"/>
		</xs:restriction>
