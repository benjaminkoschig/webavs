						<xs:element name="Uebermittlungsdatum" type="ebsv-c007:AHVDatumJMT_Type"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="KontrollMeldung" type="ebsv-c032:KontrollMeldung_Type"/>
		</xs:sequence>
