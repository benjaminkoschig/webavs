<?xml version="1.0" encoding="UTF-8"?>
<!-- Copyright : Centrale de Compensation AVS/AI, GenÃ¨ve (2016). -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:eahv-iv-2469-000101="http://www.eahv-iv.ch/xmlns/eahv-iv-2469-000101/1"
           xmlns:eCH-0058="http://www.ech.ch/xmlns/eCH-0058/5"
           xmlns:common="http://www.eahv-iv.ch/xmlns/eahv-iv-2469-common/1"
           xmlns:eCH-0007="http://www.ech.ch/xmlns/eCH-0007/5" xmlns:eCH-0011="http://www.ech.ch/xmlns/eCH-0011/8"
           xmlns:eCH-0044f="http://www.ech.ch/xmlns/eCH-0044/4"
           targetNamespace="http://www.eahv-iv.ch/xmlns/eahv-iv-2469-000101/1" elementFormDefault="qualified"
           attributeFormDefault="unqualified" version="1.5">
    <xs:import namespace="http://www.eahv-iv.ch/xmlns/eahv-iv-2469-common/1" schemaLocation="eahv-iv-2469-common-1-5.xsd"/>
    <xs:import namespace="http://www.ech.ch/xmlns/eCH-0058/5" schemaLocation="eCH-0058-5-0.xsd"/>
    <xs:import namespace="http://www.ech.ch/xmlns/eCH-0044/4" schemaLocation="eCH-0044-4-0.xsd"/>
    <xs:import namespace="http://www.ech.ch/xmlns/eCH-0007/5" schemaLocation="eCH-0007-5-0.xsd"/>
    <xs:import namespace="http://www.ech.ch/xmlns/eCH-0011/8" schemaLocation="eCH-0011-8-0.xsd"/>
	<xs:complexType name="headerType">
		<xs:complexContent>
            <xs:extension base="common:commonHeaderType">
                <xs:sequence>
					<xs:element name="subMessageType" type="eCH-0058:subMessageTypeType" fixed="000101"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>	
    <xs:complexType name="personalCalculationElementsType">
        <xs:sequence>
            <xs:element name="pensionCategory" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:choice>
                        <xs:element name="pension" type="eahv-iv-2469-000101:pensionType"/>
                        <xs:element name="noPension" type="xs:string" minOccurs="1" maxOccurs="1" fixed=""/>
                    </xs:choice>
                </xs:complexType>
            </xs:element>
            <xs:element name="hcLcaAllowance" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="lucrativeGrossIncome" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="hypotheticalGrossIncome" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="totalPension" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="lppPension" type="xs:unsignedInt" minOccurs="0" maxOccurs="1"/>
            <xs:element name="foreignPension" type="xs:unsignedInt" minOccurs="0" maxOccurs="1"/>
            <xs:element name="otherIncomes" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="lppWithdrawalAmount" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="patientContributionCategory" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:whiteSpace value="collapse"/>
                        <xs:enumeration value="INSIDE_RESIDENCE_COSTS"/>
                        <xs:enumeration value="IN_ADDITION_RESIDENCE_COSTS"/>
                        <xs:enumeration value="IGNORED"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="hcFlatHelp" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="hcEffectiveHelp" type="xs:unsignedInt" minOccurs="0" maxOccurs="1"/>
            <xs:element name="otherExpenses" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="residenceCosts" type="eahv-iv-2469-000101:residenceCostsType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="compensationOfficeType">
        <xs:sequence>
            <xs:element name="compensationOffice" type="xs:int" minOccurs="1" maxOccurs="1"/>
            <xs:element name="compensationAgency" type="xs:int" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="pensionType">
        <xs:sequence>
            <xs:element name="compensationOffice" type="eahv-iv-2469-000101:compensationOfficeType" minOccurs="1"
                        maxOccurs="1"/>
            <xs:element name="avsAipension" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="disabledAllowance" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="dailyAllowance" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="residenceCostsType">
        <xs:sequence>
            <xs:element name="residenceCostsLodging" type="xs:unsignedInt" minOccurs="0" maxOccurs="1"/>
            <xs:element name="residenceCostsCare" type="xs:unsignedInt" minOccurs="0" maxOccurs="1"/>
            <xs:element name="residenceCostsAssistance" type="xs:unsignedInt" minOccurs="0" maxOccurs="1"/>
            <xs:element name="residenceCostsPatientContribution" type="xs:unsignedInt" minOccurs="0"
                        maxOccurs="1"/>
            <xs:element name="residenceCostsTotal" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="residenceCostsConsidered" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="residencePatientContribution" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="residencePatientExpenses" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="transitionalProvisionType">
        <xs:sequence>
            <xs:element name="increaseMaxRent" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
            <xs:element name="elReform" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="elAmountsType">
        <xs:sequence>
            <xs:element name="amountNoHC" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="amountWithHC" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="elLimit" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:int">
                        <xs:enumeration value="0"/>
                        <xs:enumeration value="1"/>
                        <xs:enumeration value="2"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="calculationElementsType">
        <xs:sequence>
            <xs:element name="otherWealth" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="divestedWealth" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="otherDebts" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="wealthDeductible" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="wealthConsidered" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="wealthIncome" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="usufructIncome" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="wealthIncomeConsidered" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="incomeConsideredTotal" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="wealthIncomeRate" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:fractionDigits value="2"/>
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="100"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="vitalNeeds" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="children" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="realProperty" type="eahv-iv-2469-000101:realPropertyType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="housingOwner" type="eahv-iv-2469-000101:housingOwnerType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="rents" type="eahv-iv-2469-000101:rentsType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="realPropertyType">
        <xs:sequence>
            <xs:element name="realProperty" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="mortgageDebts" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="propertyIncome" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="mortgageInterest" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="maintenanceFees" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="interestFeesEligible" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="housingOwnerType">
        <xs:sequence>
            <xs:element name="selfInhabitedProperty" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="selfInhabitedPropertyDeductible" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:unsignedInt">
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="300000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="rentalValue" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="rentsType">
        <xs:sequence>
            <xs:element name="grossRental" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="rentCategory" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:whiteSpace value="collapse"/>
                        <xs:enumeration value="NO_RENT"/>
                        <xs:enumeration value="ANNUAL_GROSS"/>
                        <xs:enumeration value="RENTAL_VALUE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="rentGrossTotal" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="rentGrossTotalPart" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
            <xs:element name="maxRent" type="xs:unsignedInt" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="personsType">
        <xs:sequence>
            <xs:element name="person" type="eahv-iv-2469-000101:personType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="personType">
        <xs:sequence>
            <xs:element name="vn" type="eCH-0044f:vnType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="representative" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
            <xs:element name="pensionKind" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:int">
                        <xs:enumeration value="10"/>
                        <xs:enumeration value="13"/>
                        <xs:enumeration value="14"/>
                        <xs:enumeration value="15"/>
                        <xs:enumeration value="16"/>
                        <xs:enumeration value="20"/>
                        <xs:enumeration value="23"/>
                        <xs:enumeration value="24"/>
                        <xs:enumeration value="25"/>
                        <xs:enumeration value="26"/>
                        <xs:enumeration value="33"/>
                        <xs:enumeration value="34"/>
                        <xs:enumeration value="35"/>
                        <xs:enumeration value="45"/>
                        <xs:enumeration value="50"/>
                        <xs:enumeration value="54"/>
                        <xs:enumeration value="55"/>
                        <xs:enumeration value="70"/>
                        <xs:enumeration value="74"/>
                        <xs:enumeration value="75"/>
                        <xs:enumeration value="81"/>
                        <xs:enumeration value="82"/>
                        <xs:enumeration value="83"/>
                        <xs:enumeration value="84"/>
                        <xs:enumeration value="85"/>
                        <xs:enumeration value="86"/>
                        <xs:enumeration value="87"/>
                        <xs:enumeration value="88"/>
                        <xs:enumeration value="89"/>
                        <xs:enumeration value="91"/>
                        <xs:enumeration value="92"/>
                        <xs:enumeration value="93"/>
                        <xs:enumeration value="94"/>
                        <xs:enumeration value="95"/>
                        <xs:enumeration value="96"/>
                        <xs:enumeration value="97"/>
                        <xs:enumeration value="991"/>
                        <xs:enumeration value="992"/>
                        <xs:enumeration value="993"/>
                        <xs:enumeration value="994"/>
                        <xs:enumeration value="999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="vitalNeedsCategory" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:whiteSpace value="collapse"/>
                        <xs:enumeration value="NO_NEEDS"/>
                        <xs:enumeration value="ALONE"/>
                        <xs:enumeration value="COUPLE"/>
                        <xs:enumeration value="CHILD"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="maritalStatus" type="eCH-0011:maritalStatusType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="housingMode" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:whiteSpace value="collapse"/>
                        <xs:enumeration value="DOMICILE"/>
                        <xs:enumeration value="RESIDENCE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="legalAddress" type="eahv-iv-2469-000101:addressType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="livingAddress" type="eahv-iv-2469-000101:addressType" minOccurs="0" maxOccurs="1"/>
            <xs:element name="personalCalculationElements" type="eahv-iv-2469-000101:personalCalculationElementsType"
                        minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="addressType">
        <xs:sequence>
            <xs:element name="canton" type="eCH-0007:cantonAbbreviationType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="municipality" type="eCH-0007:municipalityIdType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="caseType">
        <xs:sequence>
            <xs:element name="businessCaseIdRPC" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="100"/>
                        <xs:pattern value="[A-Za-z0-9]+"/>
                        <xs:whiteSpace value="collapse"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="decisions" type="eahv-iv-2469-000101:decisionsType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="decisionsType">
        <xs:sequence>
            <xs:element name="decision" type="eahv-iv-2469-000101:decisionType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="decisionType">
        <xs:complexContent>
            <xs:extension base="common:commonDecisionType">
                <xs:sequence>
                    <xs:element name="decisionKind" type="eahv-iv-2469-000101:decisionKind" minOccurs="1"
                                maxOccurs="1"/>
                    <xs:element name="decisionCause" type="common:decisionCause" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="validTo" type="xs:gYearMonth" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="transitionalProvision" type="eahv-iv-2469-000101:transitionalProvisionType"
                                minOccurs="0" maxOccurs="1"/>
                    <xs:element name="elAmounts" type="eahv-iv-2469-000101:elAmountsType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="calculationElements" type="eahv-iv-2469-000101:calculationElementsType"
                                minOccurs="1" maxOccurs="1"/>
                    <xs:element name="persons" type="eahv-iv-2469-000101:personsType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="decisionIdPartnerDecision" type="common:decisionIdType" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:simpleType name="decisionKind">
        <xs:restriction base="xs:integer">
            <xs:enumeration value="2"/>
            <xs:enumeration value="3"/>
            <xs:enumeration value="6"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="contentType">
        <xs:sequence>
            <xs:element name="case" type="eahv-iv-2469-000101:caseType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="message">
        <xs:complexType>
            <xs:sequence>
				<xs:element name="header" type="eahv-iv-2469-000101:headerType"/>
                <xs:element name="content" type="eahv-iv-2469-000101:contentType"/>
            </xs:sequence>
            <xs:attribute name="minorVersion" type="xs:integer" use="required"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
