<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
		   xmlns:eCH-0104="http://www.ech.ch/xmlns/eCH-0104/4"
		   xmlns:eCH-0007="http://www.ech.ch/xmlns/eCH-0007/6"
		   xmlns:eCH-0044="http://www.ech.ch/xmlns/eCH-0044-f/4"
		   xmlns:eCH-0097="http://www.ech.ch/xmlns/eCH-0097/3"
		   targetNamespace="http://www.ech.ch/xmlns/eCH-0104/4" elementFormDefault="qualified" attributeFormDefault="unqualified" version="4.1">
	<xs:annotation>
		<xs:documentation xml:lang="fr">Schéma pour l'échange de données entre les caisses d'allocations familiales et la Centrale de Compensation</xs:documentation>
		<xs:documentation xml:lang="de">Datenaustauschschema zwischen den Familienausgleichskassen und der zentralen Ausgleichsstelle</xs:documentation>
	</xs:annotation>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0007/6" schemaLocation="eCH-0007-6-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0097/3" schemaLocation="eCH-0097-3-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0044-f/4" schemaLocation="eCH-0044-4-0f.xsd"/>
	<xs:simpleType name="dateType">
		<xs:restriction base="xs:date">
			<xs:minInclusive value="1980-01-01"/>
			<xs:maxInclusive value="2099-12-31"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="yearMonthType">
		<xs:restriction base="xs:gYearMonth">
			<xs:minInclusive value="1980-01"/>
			<xs:maxInclusive value="2099-12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="internalOfficeReference">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Référence interne des caisses (champ facultatif et renseigné par les caisses)</xs:documentation>
			<xs:documentation xml:lang="de">Kasseninterne Meldungskennzeichnung (fakultatives Feld, kann durch die Kasse ausgefüllt werden)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
				<xs:maxLength value="120"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:complexType name="deliveryOfficeType">
		<xs:sequence>
			<xs:element name="officeIdentifier">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Identifiant de la caisse</xs:documentation>
					<xs:documentation xml:lang="de">Kennzeichnung der Kasse</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[0-9]{1,3}|ALK[0-9]{1,2}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="branch" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Agence</xs:documentation>
					<xs:documentation xml:lang="de">Zweigstelle</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:totalDigits value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="deliveryOffice" type="eCH-0104:deliveryOfficeType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Caisse qui envoie/reçoit l'annonce</xs:documentation>
			<xs:documentation xml:lang="de">Kasse, welche die Meldung schickt/bekommt</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="legalOffice">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Entité juridique (caisse) qui est responsable de l'allocation familiale</xs:documentation>
			<xs:documentation xml:lang="de">Für die Familienzulage zuständigen juristischen Einheit (Kasse)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="officeIdentifier">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Identifiant de la caisse</xs:documentation>
						<xs:documentation xml:lang="de">Kennzeichnung der Kasse</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="[0-9]{3}\.[0-9]{3}|ALK\.[0-9]{3}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="delegated" type="xs:boolean">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Indique si la caisse délègue le traitement du dossier</xs:documentation>
			<xs:documentation xml:lang="de">Hinweis über delegierte Bearbeitung des Dossiers</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="contact">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Institution à contacter pour le suivi du dossier</xs:documentation>
			<xs:documentation xml:lang="de">Kontaktstelle für die Nachbearbeitung des Dossiers</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element name="employer">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Employeur</xs:documentation>
						<xs:documentation xml:lang="de">Arbeitgeber</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="uid" type="eCH-0097:uidStructureType" minOccurs="0">
								<xs:annotation>
									<xs:documentation xml:lang="fr">Identifiant OFS de l'entreprise</xs:documentation>
									<xs:documentation xml:lang="de">Firmen BFS-Id</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="name">
								<xs:annotation>
									<xs:documentation xml:lang="fr">Nom de l'employeur</xs:documentation>
									<xs:documentation xml:lang="de">Name des Arbeitgebers</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="100"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:choice>
								<xs:element name="contactInformation">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Information de contact</xs:documentation>
										<xs:documentation xml:lang="de">Kontaktinformation</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:maxLength value="100"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="webSite">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Page web avec informations de contact</xs:documentation>
										<xs:documentation xml:lang="de">Webseite mit Kontaktinformationen</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:anyURI">
											<xs:maxLength value="100"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
							</xs:choice>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="recordNumber">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Numéro d'enregistrement</xs:documentation>
			<xs:documentation xml:lang="de">Recordnummer</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:unsignedLong">
				<xs:totalDigits value="16"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="familyAllowanceType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Genre d'allocation familiale</xs:documentation>
			<xs:documentation xml:lang="de">Arten von Familienzulagen</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="01">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation de naissance</xs:documentation>
						<xs:documentation xml:lang="de">Geburtszulage</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="02">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation d'adoption</xs:documentation>
						<xs:documentation xml:lang="de">Adoptionszulage</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="03">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation différentielle sur une allocation de naissance</xs:documentation>
						<xs:documentation xml:lang="de">Differenz Geburtszulage</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="04">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation différentielle sur une allocation d’adoption</xs:documentation>
						<xs:documentation xml:lang="de">Differenz Adoptionszulage</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="10">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation pour enfant</xs:documentation>
						<xs:documentation xml:lang="de">Kinderzulage</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="11">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation pour enfant avec supplément plusieurs enfants</xs:documentation>
						<xs:documentation xml:lang="de">Kinderzulage mit Zuschuss für eine kinderreiche Familie</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="12">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation pour enfant incapable d'exercer une activité lucrative (jusqu'à 20 ans)</xs:documentation>
						<xs:documentation xml:lang="de">Kinderzulage für ein Kind, das erwerbsunfähig ist (bis 20 Jahre)</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="13">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation pour enfant incapable d’exercer une activité lucrative avec supplément plusieurs enfants</xs:documentation>
						<xs:documentation xml:lang="de">Kinderzulage für ein Kind, das erwerbsunfähig ist mit Zuschuss kinderreiche Familie</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="20">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation de formation professionnelle</xs:documentation>
						<xs:documentation xml:lang="de">Ausbildungszulage</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="21">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation de formation professionnelle avec supplément plusieurs enfants</xs:documentation>
						<xs:documentation xml:lang="de">Ausbildungszulage mit Zuschuss für eine kinderreiche Familie</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="22">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation avec supplément pour formation professionnelle anticipée</xs:documentation>
						<xs:documentation xml:lang="de">Zulage mit Zuschlag für eine vorgezogene Ausbildung</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="23">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation pour formation professionnelle anticipée avec supplément plusieurs enfants</xs:documentation>
						<xs:documentation xml:lang="de">Zulage mit Zuschuss für eine vorgezogene Ausbildung und Zuschuss kinderreiche Familie</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="30">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation différentielle</xs:documentation>
						<xs:documentation xml:lang="de">Differenzzulage</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="31">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation différentielle internationale</xs:documentation>
						<xs:documentation xml:lang="de">Internationale Differenzzulage</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="32">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Supplément pour enfant IJAI</xs:documentation>
						<xs:documentation xml:lang="de">Kindergeld gestützt auf das IVG </xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="vn" type="eCH-0044:vnType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Numéro d'assuré (NAVS13)</xs:documentation>
			<xs:documentation xml:lang="de">Versichertennummer (AHVN13)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="newVn" type="eCH-0044:vnType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Nouveau numéro d'assuré (NAVS13)</xs:documentation>
			<xs:documentation xml:lang="de">Neue Versichertennummer (AHVN13)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dateOfDeath" type="eCH-0044:datePartiallyKnownType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Date de décès</xs:documentation>
			<xs:documentation xml:lang="de">Sterbedatum</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="lawType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="01">
				<xs:annotation>
					<xs:documentation xml:lang="fr">LAFam</xs:documentation>
					<xs:documentation xml:lang="de">FamZG</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="02">
				<xs:annotation>
					<xs:documentation xml:lang="fr">LACI</xs:documentation>
					<xs:documentation xml:lang="de">AVIG</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="03">
				<xs:annotation>
					<xs:documentation xml:lang="fr">LFA - régime plaine</xs:documentation>
					<xs:documentation xml:lang="de">FLG - Regelung Talgebiet</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="04">
				<xs:annotation>
					<xs:documentation xml:lang="fr">LFA - régime montagne</xs:documentation>
					<xs:documentation xml:lang="de">FLG - Regelung Berggebiet</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="05">
				<xs:annotation>
					<xs:documentation xml:lang="fr">LAI</xs:documentation>
					<xs:documentation xml:lang="de">IVG</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="legalBasis">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="law" type="eCH-0104:lawType">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Base légale</xs:documentation>
						<xs:documentation xml:lang="de">Gesetzliche Grundlage</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="canton" type="eCH-0007:cantonAbbreviationType" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Canton dont la législation s'applique</xs:documentation>
						<xs:documentation xml:lang="de">Kanton, dessen Gesetzgebung anwendbar ist</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="validityPeriod">
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element name="start" type="eCH-0104:dateType">
						<xs:annotation>
							<xs:documentation xml:lang="fr">Date de début de droit</xs:documentation>
							<xs:documentation xml:lang="de">Anfangsdatum</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="end" type="eCH-0104:dateType">
						<xs:annotation>
							<xs:documentation xml:lang="fr">Date de fin de droit</xs:documentation>
							<xs:documentation xml:lang="de">Enddatum</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:sequence>
					<xs:element name="controlMonth" type="eCH-0104:yearMonthType">
						<xs:annotation>
							<xs:documentation xml:lang="fr">Mois de contrôle</xs:documentation>
							<xs:documentation xml:lang="de">Kontrollmonat</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="numberOfWorkdays">
						<xs:annotation>
							<xs:documentation xml:lang="fr">Nombre de jours ouvrables où l'allocation a été perçue</xs:documentation>
							<xs:documentation xml:lang="de">Anzahl der Arbeitstage, für welche die Leistung beansprucht wurde</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:unsignedInt">
								<xs:maxInclusive value="31"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="familialStatus">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Statut familial du bénéficiaire</xs:documentation>
			<xs:documentation xml:lang="de">Familienstatus des Bezügers</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="10">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Mère</xs:documentation>
						<xs:documentation xml:lang="de">Mutter</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="11">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Belle-mère</xs:documentation>
						<xs:documentation xml:lang="de">Stiefmutter</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="12">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Mère nourricière (enfants recueillis)</xs:documentation>
						<xs:documentation xml:lang="de">Pflegemutter (Kinder aufgenommen)</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="13">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Soeur</xs:documentation>
						<xs:documentation xml:lang="de">Schwester</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="14">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Grand-mère</xs:documentation>
						<xs:documentation xml:lang="de">Grossmutter</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="20">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Père</xs:documentation>
						<xs:documentation xml:lang="de">Vater</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="21">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Beau-père</xs:documentation>
						<xs:documentation xml:lang="de">Stiefvater</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="22">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Père nourricier (enfants recueillis)</xs:documentation>
						<xs:documentation xml:lang="de">Pflegevater (Kinder aufgenommen)</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="23">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Frère</xs:documentation>
						<xs:documentation xml:lang="de">Bruder</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="24">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Grand-père</xs:documentation>
						<xs:documentation xml:lang="de">Grossvater</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="occupationStatus">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Statut de l'activité du bénéficiaire</xs:documentation>
			<xs:documentation xml:lang="de">Erwerbsstatus des Bezügers</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="01">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Salarié</xs:documentation>
						<xs:documentation xml:lang="de">Arbeitnehmende</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="02">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Indépendant</xs:documentation>
						<xs:documentation xml:lang="de">Selbständig erwerbend</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="03">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Sans activité lucrative</xs:documentation>
						<xs:documentation xml:lang="de">Nicht erwerbstätig</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="04">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Chômeur</xs:documentation>
						<xs:documentation xml:lang="de">Arbeitslos</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="05">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Agriculteur (soumis à la LFA)</xs:documentation>
						<xs:documentation xml:lang="de">Landwirt (dem FLG unterstellt)</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="06">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Membre de la famille travaillant dans l'exploitation (soumis à la LFA)</xs:documentation>
						<xs:documentation xml:lang="de">Im Betrieb mitarbeitendes Familienmitglied (dem FLG unterstellt)</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="07">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Travailleur agricole (soumis à la LFA)</xs:documentation>
						<xs:documentation xml:lang="de">Landwirtschaftlicher Arbeitnehmer (dem FLG unterstellt)</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="08">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Bénéficiaire IJAI</xs:documentation>
						<xs:documentation xml:lang="de">IVTG-Bezüger</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="09">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Travailleur sans employeur (TSE)</xs:documentation>
						<xs:documentation xml:lang="de">Arbeitnehmer ohne beitragspflichtigen Arbeitgeber (ANobAG)</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="comment">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Remarque</xs:documentation>
			<xs:documentation xml:lang="de">Bemerkung</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="01">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Restitutif de prestation</xs:documentation>
						<xs:documentation xml:lang="de">Rückerstattungsanspruch für die Leistung</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="02">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Salaire oscillant autour de la limite de droit à l'allocation</xs:documentation>
						<xs:documentation xml:lang="de">Unregelmässige Einkommen, welche um die notwendige Minimalhöhe des Anspruchrechts schwanken</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="specialTreatment">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Traitement spécial</xs:documentation>
			<xs:documentation xml:lang="de">Besondere Verarbeitung</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="A">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocation chômage payée en même temps qu’une prestation pour enfant IJAI</xs:documentation>
						<xs:documentation xml:lang="de">AVIG-Zulage bezahlt zusammen mit dem IV-Kindergeld</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="creationDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Date de création donnée par la CdC</xs:documentation>
			<xs:documentation xml:lang="de">Datum des Eintrages gemäss Definition der Zentrale</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="mutationDate" type="xs:date">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Date de dernière modification donnée par la CdC</xs:documentation>
			<xs:documentation xml:lang="de">Datum der letzten Anpassung gemäss Definition der Zentrale</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="errorPeriodType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Période où l'allocation est en erreur</xs:documentation>
			<xs:documentation xml:lang="de">Betroffene Periode der Fehlermeldung</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="start" type="eCH-0104:dateType"/>
			<xs:element name="end" type="eCH-0104:dateType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="minimalStartFlagType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Indéterminé (dates de début identiques ou pas de date de début)</xs:documentation>
					<xs:documentation xml:lang="de">Unbestimmt (identische oder keine Beginn-Daten)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Si la caisse est celle ayant l'allocation avec la date de début la plus petite</xs:documentation>
					<xs:documentation xml:lang="de">Falls Kasse mit dem früheren Beginn des Anspruchsdatums</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Si la caisse est celle ayant l'allocation avec la date de début supérieure à celle de l'autre caisse</xs:documentation>
					<xs:documentation xml:lang="de">Falls Kasse mit dem späteren Beginn des Anspruchsdatums</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="insignificanceType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Conflit important</xs:documentation>
					<xs:documentation xml:lang="de">Relevanter Konflikt</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Conflit de moindre importance</xs:documentation>
					<xs:documentation xml:lang="de">Geringfügiger Konflikt</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="overlapInformation">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Informations pour une erreur de double paiement</xs:documentation>
			<xs:documentation xml:lang="de">Information zu einem Doppelbezug</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="deliveryOfficeConflict" type="eCH-0104:deliveryOfficeType">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Numéro de caisse en conflit</xs:documentation>
						<xs:documentation xml:lang="de">Kassennummer der Kasse in Konflikt</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="overlapPeriod" type="eCH-0104:errorPeriodType">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Période où l'allocation est en contradiction avec une autre allocation dans le registre</xs:documentation>
						<xs:documentation xml:lang="de">Die Periode, wo die Zulage im Widerspruch zu einer anderen Zulage im Register steht</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="minimalStartFlag" type="eCH-0104:minimalStartFlagType">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Statut de priorité de caisse en conflit</xs:documentation>
						<xs:documentation xml:lang="de">Prioritätsstatus der Kasse in Konflikt</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="insignificance" type="eCH-0104:insignificanceType">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Importance du conflit</xs:documentation>
						<xs:documentation xml:lang="de">Relevanz des Konflikts</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="error">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Plausibilité non respectée</xs:documentation>
			<xs:documentation xml:lang="de">Nicht eingehaltenen Plausibilität</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="code">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Numéro de la plausibilité non respectée</xs:documentation>
						<xs:documentation xml:lang="de">Nummer der nicht eingehaltenen Plausibilität</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:unsignedInt">
							<xs:totalDigits value="3"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="errorPeriod" type="eCH-0104:errorPeriodType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Période où l'allocation est en erreur (mais sans chevauchement avec une autre allocation)</xs:documentation>
						<xs:documentation xml:lang="de">Betroffene Periode der Fehlermeldung (aber ohne Überschneidung mit einer anderen Zulage)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="eCH-0104:overlapInformation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="receiptReturnCodeType">
		<xs:restriction base="xs:unsignedInt">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Exécuté</xs:documentation>
					<xs:documentation xml:lang="de">Verarbeitet</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Exécuté, mais annonce mise en erreur</xs:documentation>
					<xs:documentation xml:lang="de">Verarbeitet, Meldung weist aber Fehler auf</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Non exécuté, annonce refusée</xs:documentation>
					<xs:documentation xml:lang="de">Nicht verarbeitet, Meldung zurückgewiesen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Annonce annulée</xs:documentation>
					<xs:documentation xml:lang="de">Meldung aufgehoben</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Annonce mise en attente</xs:documentation>
					<xs:documentation xml:lang="de">Zwischengespeicherte Meldung</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Rappel</xs:documentation>
					<xs:documentation xml:lang="de">Reminder</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="noticeReturnCodeType">
		<xs:restriction base="xs:unsignedInt">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Situation normalisée</xs:documentation>
					<xs:documentation xml:lang="de">Situation bereinigt</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Situation conflictuelle</xs:documentation>
					<xs:documentation xml:lang="de">Situation widersprüchlich</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Rappel</xs:documentation>
					<xs:documentation xml:lang="de">Reminder</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="UPIReturnCodeType">
		<xs:restriction base="xs:unsignedInt">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Exécuté</xs:documentation>
					<xs:documentation xml:lang="de">Verarbeitet</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Exécuté, mais annonce mise en erreur</xs:documentation>
					<xs:documentation xml:lang="de">Verarbeitet, Meldung weist aber Fehler auf</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="registerStatusReturnCodeType">
		<xs:restriction base="xs:unsignedInt">
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Rappel</xs:documentation>
					<xs:documentation xml:lang="de">Reminder</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="canceledType">
		<xs:restriction base="xs:unsignedInt">
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Annulée</xs:documentation>
					<xs:documentation xml:lang="de">Annulliert</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Maintenue</xs:documentation>
					<xs:documentation xml:lang="de">Halten der Meldung</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="enumYesNoAllType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="yes">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Oui</xs:documentation>
					<xs:documentation xml:lang="de">Ja</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="no">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Non</xs:documentation>
					<xs:documentation xml:lang="de">Nein</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="all">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Tous</xs:documentation>
					<xs:documentation xml:lang="de">Alles</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="commandDetails">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Détails de la commande</xs:documentation>
			<xs:documentation xml:lang="de">Beantragte Version</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="inProgress" type="eCH-0104:enumYesNoAllType">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocations présentes ou futures / dans le passé / toutes</xs:documentation>
						<xs:documentation xml:lang="de">Noch laufende (Enddatum in Zukunft) / abgelaufene (Enddatum in der Vergangenheit) Zulagen / alle</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="conflict" type="eCH-0104:enumYesNoAllType">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocations en conflit / sans conflit / toutes</xs:documentation>
						<xs:documentation xml:lang="de">Zulagen mit Konflikt / ohne Konflikt / alle</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="canceled" type="eCH-0104:enumYesNoAllType">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Allocations annulées / pas annulées / toutes</xs:documentation>
						<xs:documentation xml:lang="de">Annullierte Zulagen / nicht annullierte Zulagen / alle</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
