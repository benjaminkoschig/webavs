	<!-- Annonce de remboursement/transfert ================================================-->
	<xs:complexType name="RRMeldungRueckerstattungUeberweisung_Type">
		<xs:annotation>
			<xs:documentation xml:lang="fr"> code application = 47</xs:documentation>
			<xs:documentation xml:lang="de">Anwendungsgebiet 47</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="KasseZweigstelle" type="IrgendeineZweigstelle_weakType"/>
			<xs:element name="Meldungsnummer" type="Meldungsnummer_Type"/>
			<xs:element name="KasseneigenerHinweis" type="KasseneigenerHinweis_Type" minOccurs="0"/>
			<xs:element name="LeistungsberechtigtePerson">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Versichertennummer" type="Versichertennummer11oder13Stellen_Type"/>
						<xs:element name="WohnkantonStaat" type="WohnkantonStaatBSV_weakType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Leistungsbeschreibung">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="DatumRueckerstattungUeberweisung" type="AHVDatumJM_Type"/>
						<xs:element name="Betrag" type="BetragFRRP6_Type">
							<xs:annotation>
								<xs:documentation>FFFFFF.CC </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Mutationscode">
							<xs:annotation>
								<xs:documentation xml:lang="fr">R=remboursement   T=transfert</xs:documentation>
								<xs:documentation xml:lang="de">R=Rückerstattung T=Überweisung</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="R"/>
									<xs:enumeration value="T"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Berichtsmonat" type="AHVDatumJM_Type"/>
		</xs:sequence>
	</xs:complexType>
	<!-- Réponse de la centrale aux caisses ====================================================== -->
	<xs:complexType name="RRRueckmeldung_Type">
		<xs:annotation>
			<xs:documentation xml:lang="fr">
                Réponse de la Centrale aux Caisses après traitement d'une annonce (9e + 10e révision, code
                d'application: 50)
            </xs:documentation>
			<xs:documentation xml:lang="de">
                Rückmeldung der ZAS an die AK nach Verarbeitung der Meldung (9. und 10.Revision, Anwendungsgebiet = 50)
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="KasseZweigstelle" type="IrgendeineZweigstelle_weakType"/>
			<xs:element name="Meldungsnummer" type="Meldungsnummer_Type"/>
			<xs:element name="KasseneigenerHinweis" type="KasseneigenerHinweis_Type" minOccurs="0"/>
			<xs:element name="LeistungsberechtigtePerson">
				<xs:annotation>
					<xs:documentation>Données conernant l'ayant droit à la prestation</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Versichertennummer" type="Versichertennummer11oder13Stellen_Type">
							<xs:annotation>
								<xs:documentation xml:lang="fr">Numéro d'assuré de l'ayant droit</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NeueVersichertennummer" type="Versichertennummer11oder13Stellen_Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="fr">
                                    Nouveau numéro d'assuré de l'ayant droit à la prestation
                                </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="FamilienAngehoerige" type="FamilienAngehoerige_Type"/>
						<xs:element name="Zivilstand" type="ZivilstandNr_Type" minOccurs="0"/>
						<xs:element name="WohnkantonStaat" type="WohnkantonStaatBSV_weakType" minOccurs="0"/>
						<xs:element name="istFluechtling" type="xs:boolean" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Leistungsbeschreibung">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Leistungsart" type="Leistungsart_alle_Revisionen_strongType"/>
						<xs:element name="Anspruchsbeginn" type="AHVDatumJM_Type" minOccurs="0"/>
						<xs:element name="Monatsbetrag" type="BetragFR_Type" minOccurs="0"/>
						<xs:element name="MonatsbetragErsetzteOrdentlicheRente" type="BetragFR_Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>
                                    Mensualité de la rente ordinaire remplacée en francs (9e révision)
                                </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Anspruchsende" type="AHVDatumJM_Type" minOccurs="0"/>
						<xs:element name="Mutationscode" type="Grundabgang_Type" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Berichtsmonat" type="AHVDatumJM_Type"/>
			<xs:element name="AntwortZAS">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Verarbeitungscode">
							<xs:annotation>
								<xs:documentation>code de traitement: 0 = traité</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:unsignedByte">
									<xs:minInclusive value="0"/>
									<xs:maxInclusive value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Meldungsart">
							<xs:simpleType>
								<xs:restriction base="xs:unsignedByte">
									<xs:minInclusive value="0"/>
									<xs:maxInclusive value="4"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Etat des rentes selon 9ème révision ================================================= -->
	<xs:complexType name="RRBestandesmeldung9_Type">
		<xs:annotation>
			<xs:documentation xml:lang="fr">
                annonce de l'état des rentes de la CdC aux Caisses dans le but d'établir la concordance (code
                d'application = 51; 9e révision)
            </xs:documentation>
			<xs:documentation xml:lang="de">
                Bestandesmeldung der ZAS an die AK zwecks Übereinstimmungskontrolle (9.Revision. Anwendungsgebiet = 51)
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="OrdentlicheRente" type="RRBestandesmeldungO9_Type"/>
				<xs:element name="AusserordentlicheRente" type="RRBestandesmeldungAO9_Type"/>
				<xs:element name="Hilflosenentschaedigung" type="RRBestandesmeldungHE9_Type">
					<xs:annotation>
						<xs:documentation>API</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="ZusaetzlicheAngabenZAS">
				<xs:annotation>
					<xs:documentation>données supplémentaires (fournies par la CdC)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Namensangaben" type="NameUndVorname_Type"/>
						<xs:element name="Heimatstaat" type="WohnkantonStaatBSV_weakType"/>
						<xs:element name="ProzentualAnteil" type="ProzentualAnteil_Type"/>
						<xs:element name="BisherigeWerte" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="fr">Valeurs anciennes lors de la conversion des rentes</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="DurchschnittlichesJahreseinkommen" type="BetragFR_Type" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="fr">Ancien revenu annuel moyen déterminant</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="MonatsbetragErsetzteOrdentlicheRente" type="BetragFR_Type" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="fr">Ancienne RO remplacée      </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Monatsbetrag" type="BetragFR_Type" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="fr">Ancien montant mensuel</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="SonderfallcodeRente" type="SonderfallcodeRente_Type" minOccurs="0" maxOccurs="5">
										<xs:annotation>
											<xs:documentation xml:lang="fr">Anciens cas spéciaux</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="DJEohneErziehungsgutschrift" type="BetragFR_Type" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="fr">Ancien revenu annuel moyen sans bonifications pour tâches éducatives</xs:documentation>
											<xs:documentation/>
										</xs:annotation>
									</xs:element>
									<xs:element name="AngerechneteErziehungsgutschrift" type="BetragFR_Type" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="fr">Anciennes bonifications pour tâches éducatives moyennes prises en compte</xs:documentation>
											<xs:documentation>en francs</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Aufschubszuschlag" type="BetragFR_Type" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="fr">Ancien supplément d'ajournement</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="BemerkungenZAS" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="fr">Observations de la Centrale lors de la conversion des rentes</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="0"/>
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
