										<xs:enumeration value="enfant"/>
										<xs:enumeration value="ex_epouse"/>
										<xs:enumeration value="ex_epoux"/>
										<xs:enumeration value="nouvel_epx"/>
										<xs:enumeration value="nouvelle_epouse"/>
										<xs:enumeration value="nouvel_epx_ex"/>
										<xs:enumeration value="nouvelle_epouse_ex"/>
										<xs:enumeration value="mere_nat"/>
										<xs:enumeration value="pere_nat"/>
										<xs:enumeration value="enf_nat"/>
										<xs:enumeration value="enf_ex"/>
										<xs:enumeration value="pere_epoux"/>
										<xs:enumeration value="mere_epouse"/>
										<xs:enumeration value="part"/>
										<xs:enumeration value="ex_part"/>
										<xs:enumeration value="ex_ex_epouse"/>
										<xs:enumeration value="ex_ex_epoux"/>
										<xs:enumeration value="enf_ex_ex"/>
										<xs:enumeration value="mere_nat_ex"/>
										<xs:enumeration value="pere_nat_ex"/>
										<xs:enumeration value="enf_nat_ex"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="sexe" minOccurs="0">
