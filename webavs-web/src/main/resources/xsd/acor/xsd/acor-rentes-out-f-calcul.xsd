						<xs:sequence>
							<xs:element name="genre">
								<xs:annotation>
									<xs:documentation>genre de demande:
                                        v = vieillesse ;
                                        s = survivant ;
                                        i = invalidité ;
                                        c = communication de périodes ;
                                        r = remboursement de cotisations
                                        t = transfert de cotisations
                                        splitt = splitting
                                    </xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="v"/>
										<xs:enumeration value="s"/>
										<xs:enumeration value="i"/>
										<xs:enumeration value="c"/>
										<xs:enumeration value="r"/>
										<xs:enumeration value="t"/>
										<xs:enumeration value="splitt"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="label" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>label pour un titre dans la feuille de calcul ACOR
                                    </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="requerant" type="id_acor">
								<xs:annotation>
									<xs:documentation>id du requérant</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="date_traitement" type="AAAAMMJJ" minOccurs="0">
								<xs:annotation>
									<xs:documentation>date de traitement</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="caisse">
								<xs:annotation>
									<xs:documentation>*annonce CdC /
                                        Meldung ZAS
                                        Numéro de caisse et agence
                                        Nummer der AK und Zweigstelle
                                    </xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:int">
										<xs:minInclusive value="0"/>
