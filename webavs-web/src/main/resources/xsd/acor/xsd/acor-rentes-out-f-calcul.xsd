									<xs:documentation>Nationalität</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="code" type="CodePays"/>
										<xs:element name="nom" type="xs:string" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="domicile" minOccurs="0">
