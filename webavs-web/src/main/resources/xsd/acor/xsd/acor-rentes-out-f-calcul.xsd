						<xs:annotation>
							<xs:documentation>rente pour IF</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="class">@io.swagger.v3.oas.annotations.media.Schema(name="RenteIF",description = "rente pour IF")</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
						<xs:sequence>
							<xs:element name="genre" type="GenrePrestation">
								<xs:annotation>
									<xs:documentation>* annonce CdC /
                                        Meldung ZAS
