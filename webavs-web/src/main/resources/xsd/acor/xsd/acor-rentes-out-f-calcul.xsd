									<xs:documentation>date de dépôt de la demande</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="date_determinante_prescription" type="AAAAMMJJ" minOccurs="0">
								<xs:annotation>
									<xs:documentation>date déterminante pour le calcul de la prescription
                                    </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="date_prescription" type="AAAAMMJJ" minOccurs="0">
								<xs:annotation>
									<xs:documentation>date de prescription</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="previsionnel" type="xs:boolean">
