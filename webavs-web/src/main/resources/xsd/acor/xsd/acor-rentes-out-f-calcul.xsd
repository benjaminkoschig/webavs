													<xs:restriction base="xs:string">
														<xs:enumeration value="navs"/>
														<xs:enumeration value="navs11"/>
														<xs:enumeration value="autre"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="valide" type="xs:boolean" use="required">
												<xs:annotation>
													<xs:documentation>Dit si l'identifiant est un Navs valide ou non</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="nom" type="xs:string" minOccurs="0">
