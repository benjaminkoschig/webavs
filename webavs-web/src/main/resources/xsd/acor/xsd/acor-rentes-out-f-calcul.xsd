							<xs:element name="provisoire" type="xs:boolean">
								<xs:annotation>
									<xs:documentation>demande de paiement provisoire</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="assure" maxOccurs="unbounded">
