			<xs:sequence>
				<xs:element name="version_expert">
					<xs:annotation>
						<xs:documentation>version de ACOR
                            ACOR Version
                        </xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="(([0-9]\.?)*-?(DEV|SNAPSHOT)?)|(valide)"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="reference_interne" type="xs:string" minOccurs="0"/>
