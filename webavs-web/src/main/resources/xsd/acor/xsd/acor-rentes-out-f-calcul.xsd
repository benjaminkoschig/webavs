									<xs:restriction base="xs:int">
										<xs:minInclusive value="0"/>
										<xs:maxInclusive value="99"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="code_mutation" minOccurs="0">
								<xs:annotation>
									<xs:documentation>code mutation</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:int">
										<xs:minInclusive value="0"/>
										<xs:maxInclusive value="99"/>
										<!-- 2020 TODO: Attention il y a des nouveaux codes spéciaux supérieurs à 99. Sous 2020, il faut donc adapter maxInclusive -->
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="surv_inv" minOccurs="0">
								<xs:annotation>
									<xs:documentation>code survivant invalide selon annonce au RR (présent seulement pour rente de
                        survivant)
                    </xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:int">
										<xs:enumeration value="0"/>
										<xs:enumeration value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="ncpl1" type="id_acor" minOccurs="0">
								<xs:annotation>
									<xs:documentation>numéro d'assuré complémentaire 1</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ncpl2" type="id_acor" minOccurs="0">
								<xs:annotation>
									<xs:documentation>numéro d'assuré complémentaire 2</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="vact" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>valeur actuelle pour la décomposition</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:float">
							<xs:attribute name="type" type="vact" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="RH" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>montant de rente pour la décomposition</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:int">
							<xs:attribute name="type" type="montant_r_if" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="montant">
				<xs:annotation>
					<xs:documentation>* annonce CdC /
                        Meldung ZAS
                        Montant de l'indemnité forfaitaire
                        Abfindungbetrag
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="date" type="AAAAMMJJ">
				<xs:annotation>
					<xs:documentation>* annonce CdC /
                        Meldung ZAS
                        Date de droit à l'IF
                        Anspruchsbeginn
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="moment_determinant" type="AAAAMMJJ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>moment déterminant</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mois_rapport" type="AAAAMMJJ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>* annonce CdC /
                        Meldung ZAS
                        Mois de rapport
                        Berichtsmonat
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
