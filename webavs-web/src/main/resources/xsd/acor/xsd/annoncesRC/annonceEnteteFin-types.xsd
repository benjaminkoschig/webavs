		<xs:annotation>
			<xs:documentation xml:lang="fr">code application 01</xs:documentation>
			<xs:documentation xml:lang="de">Anwendungsgebiet 01</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Sender" type="xs:string"/>
			<xs:element name="KasseZweigstelle" type="IrgendeineZweigstelle_weakType" minOccurs="0"/>
