					<xsd:documentation xml:lang="de">Verwandschaftsverhältnis</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="ReceivingInstitution">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">field 1</xsd:documentation>
			<xsd:documentation xml:lang="en">e210 Receiving Institution</xsd:documentation>
			<xsd:documentation xml:lang="de">e210 empfangendes Institut</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="core:InsuranceInstitution">
					<xsd:sequence>
						<xsd:element ref="core:InstitutionType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation xml:lang="en">Typ of institution</xsd:documentation>
								<xsd:documentation xml:lang="de">Art des Versicherungsinstituts</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="core:InstitutionCode" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation xml:lang="en">code of institution</xsd:documentation>
								<xsd:documentation xml:lang="de">Kodierung des Versicherungsinstituts</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="core:InstitutionName" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation xml:lang="en">name of institution</xsd:documentation>
								<xsd:documentation xml:lang="de">Namen des Versicherungsinstituts</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="core:Country" minOccurs="0" maxOccurs="0">
							<xsd:annotation>
								<xsd:documentation xml:lang="en">Identification of the name of a country or other geographical entity as specified in ISO 3166.
Note: Use ISO 3166 two alpha country code.</xsd:documentation>
								<xsd:documentation xml:lang="de">Kennbuchstabe des Landes</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="core:InsuranceNumber" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation xml:lang="en">number of insurance</xsd:documentation>
								<xsd:documentation xml:lang="de">Nummer des Versicherungsinstituts</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="core:CaseNumber" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation xml:lang="en">case number</xsd:documentation>
								<xsd:documentation xml:lang="de">Geschäftsnummer</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="core:DossierNumber" minOccurs="0" maxOccurs="0">
							<xsd:annotation>
								<xsd:documentation xml:lang="en">dossiernumber</xsd:documentation>
								<xsd:documentation xml:lang="de">Aktenzeichen</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="core:RoleOfInstitution" minOccurs="0" maxOccurs="0">
							<xsd:annotation>
								<xsd:documentation xml:lang="en">role of the institution</xsd:documentation>
								<xsd:documentation xml:lang="de">Rolle des Versicherungsinstituts</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="core:TaxpayerNumber" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation xml:lang="en">number of the taxpayer file</xsd:documentation>
								<xsd:documentation xml:lang="de">Steuernummer</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="core:Address" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation xml:lang="en">standard address</xsd:documentation>
								<xsd:documentation xml:lang="de">Standard Adress</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="core:PhoneNumber" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation xml:lang="en">phone number</xsd:documentation>
								<xsd:documentation xml:lang="de">Telefonnummer</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
