						<xsd:documentation xml:lang="en">field 9.2</xsd:documentation>
						<xsd:documentation xml:lang="en">pension reduction</xsd:documentation>
						<xsd:documentation xml:lang="de">Leistungsminderung</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="e202:Advance" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">field 8.17</xsd:documentation>
						<xsd:documentation xml:lang="en">advance</xsd:documentation>
						<xsd:documentation xml:lang="de">Vorschuss</xsd:documentation>
