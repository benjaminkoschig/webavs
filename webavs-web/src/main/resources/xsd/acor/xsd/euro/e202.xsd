						<xsd:documentation xml:lang="en">field 7.1, 7.2, 7.3, 7.4 and 7.5</xsd:documentation>
						<xsd:documentation xml:lang="en">type of occupation</xsd:documentation>
						<xsd:documentation xml:lang="de">Herkunft des Einkommens</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="e202:SinceDate" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">field 7.1, 7.2 and 7.3</xsd:documentation>
						<xsd:documentation xml:lang="en">since date</xsd:documentation>
						<xsd:documentation xml:lang="de">Startdatum</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
