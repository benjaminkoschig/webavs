			<xsd:documentation xml:lang="de">Art der Leistungen</xsd:documentation>
			<xsd:appinfo source="http://www.gotess.org/xns/codinglink">
				<c:codelist list="BenefitType.xml" type="default"/>
			</xsd:appinfo>
