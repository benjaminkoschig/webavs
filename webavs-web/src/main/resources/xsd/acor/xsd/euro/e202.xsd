					<xsd:documentation xml:lang="en">refund of contribution</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="81">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">transfer of contributions</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
