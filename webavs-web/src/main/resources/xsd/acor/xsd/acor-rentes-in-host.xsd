               schemaLocation="./annoncesRC/CACI/annonceCI-types.xsd"/>-->
	<xs:element name="inHost" type="rentes_in_host:inHostType"/>
	<xs:complexType name="inHostType">
		<xs:annotation>
			<xs:appinfo>
				<annox:annotate target="class">@io.swagger.v3.oas.annotations.media.Schema(description = "fr : Format in des donn\u0026eacute;es du dossier.", required = false)</annox:annotate>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="rentes_in_host:Meta">
				<xs:sequence>
					<xs:element name="demande" type="rentes_in_host:demandeType"/>
					<xs:element name="assure" type="rentes_in_host:assureType" minOccurs="1" maxOccurs="unbounded"/>
					<xs:element name="enfant" type="rentes_in_host:enfantType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="famille" type="rentes_in_host:familleType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
				<xs:attribute name="versionSchema" use="required">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="([0-9]\.?)*-?(DEV|SNAPSHOT)?"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="demandeType">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:Meta">
				<xs:sequence>
					<xs:element name="navs" type="xs:long">
						<xs:annotation>
							<xs:appinfo>
								<annox:annotate target="field">@io.swagger.v3.oas.annotations.media.Schema(description = "fr : Num\u0026eacute;ro AVS (13) pour l'assur\u0026eacute; ma\u0026icirc;tre de la demande. (DEMANDE/1)",example="7560747116424", required = true)</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="caisseAgence" type="trax:IrgendeineZweigstelle_weakType"/>
					<xs:element name="adresseCaisse" type="rentes_in_host:organisationAdresseType"/>
					<xs:element name="moisRapport">
						<xs:annotation>
							<xs:documentation>
								0 = mois de traitement, 1 = mois suivant le mois de traitement.
								This information is set (as a Date) in the PoolMeldungZurZAS/lot/PoolKopf/rc:Erstellungsdatum.
								the total of amount payed (f_calcul/evenement/bases_calcul/decision/prestation/rente/versement/montant) with one month more if "moisRapport = 1"
								(idem for the coresponding field of annonce.pay : f_calcul/annexes/pay
							</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:enumeration value="0"/>
								<xs:enumeration value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="noDossier" type="xs:string" minOccurs="0"/>
					<xs:element name="typeDemande" type="rentes_in_host:typeDemandeEnum"/>
					<xs:element name="dateTraitement" type="xs:date">
						<xs:annotation>
							<xs:appinfo>
								<annox:annotate target="field">@io.swagger.v3.oas.annotations.media.Schema(description = "fr : la date de traitement (DEMANDE/3)", required = true)</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="dateDepot" type="xs:date" minOccurs="0"/>
					<xs:element name="langue">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="f"/>
								<xs:enumeration value="d"/>
								<xs:enumeration value="i"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="typeCalcul">
						<xs:annotation>
							<xs:documentation>0 = standard
                                1 = prévisionnel
                                2 = paiement provisoire
                            </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="2"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="calculRenteConjoint" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>true = la rente de vieillesse du conjoint plus jeune sera calculée en mode
                                prévisionnel
                            </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="demiRente9" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>module 9ème révision : acor calculera des demi rente de couple plutôt qu'une rente
                                de couple
                            </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="remplaceRente" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>remplacement sur demande de l'épouse des rentes de couple 11, 12, 51,52
                                remplacement sur demande des rentes simples des femmes devenues veuves avant le 1er décembre
                                1996 et dont l’échelle est celle du mari (Circ. II 8042 ss)
                            </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="secteur" type="xs:int" minOccurs="0"/>
					<xs:element name="groupe" type="xs:int" minOccurs="0"/>
					<xs:element name="visa" type="xs:string" minOccurs="0"/>
					<xs:element name="visaChef" type="xs:string" minOccurs="0"/>
					<xs:element name="libre" type="xs:string" minOccurs="0"/>
					<xs:element name="dateRepriseCaisse" type="xs:date" minOccurs="0"/>
					<xs:element name="dateDeterminantePrescription" type="xs:date" minOccurs="0"/>
					<xs:element name="dureePrescription" type="rentes_in_host:dureePrescriptionEnum" minOccurs="0"/>
					<xs:element name="dateMinMut" type="xs:date" minOccurs="0">
						<xs:annotation><xs:documentation>Aucun événement ne sera traité avant cette date</xs:documentation></xs:annotation>
					</xs:element>
					<xs:element name="dateMaxMut" type="xs:date" minOccurs="0">
						<xs:annotation><xs:documentation>Aucun événement ne sera traité après cette date</xs:documentation></xs:annotation>
					</xs:element>
					<xs:element name="demandeEuro" type="rentes_in_host:demandeEuroType" minOccurs="0"/>
					<xs:element name="montantMinimumEmployeurAPG" type="xs:int" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                                Concerne les APG : montant minimum versé à l’assuré en francs (p. ex : 10 ou 5) (mettre 0 si la caisse n'utilise pas ACOR pour les APG)
                            </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="demandeEuroType">
		<xs:sequence>
			<xs:element name="e202" type="xs:boolean" minOccurs="0"/>
			<xs:element name="e203" type="xs:boolean" minOccurs="0"/>
			<xs:element name="e205" type="xs:boolean" minOccurs="0"/>
			<xs:element name="e210" type="xs:boolean" minOccurs="0"/>
			<xs:element name="e210DateJour" type="xs:boolean" minOccurs="0"/>
			<xs:element name="institutionInstruction" type="xs:boolean" minOccurs="0"/>
			<xs:element name="swap" type="xs:boolean" minOccurs="0"/>
			<xs:element name="langue">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="f"/>
						<xs:enumeration value="d"/>
						<xs:enumeration value="i"/>
						<xs:enumeration value="e"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="dureePrescriptionEnum">
		<xs:annotation>
			<xs:documentation>Durée de la prescription en mois; 0=pas de prescription, 12=1 année selon Art. 25 LPGA,
                60=5 ans selon Art. 24 LPGA, 84=7 ans selon Art. 97 CP
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:enumeration value="0"/>
			<xs:enumeration value="12"/>
			<xs:enumeration value="60"/>
			<xs:enumeration value="84"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="typeDemandeEnum">
		<xs:annotation>
			<xs:appinfo>
				<annox:annotate target="enum">@io.swagger.v3.oas.annotations.media.Schema(description = "fr : le type de la demande (DEMANDE/2)", required = true)</annox:annotate>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="v"/>
			<xs:enumeration value="s"/>
			<xs:enumeration value="i"/>
			<xs:enumeration value="r"/>
			<xs:enumeration value="c"/>
			<xs:enumeration value="l"/>
			<xs:enumeration value="j"/>
			<xs:enumeration value="t"/>
			<xs:enumeration value="splitt"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="abstractAssureType" abstract="true">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:Meta">
				<xs:sequence>
					<xs:element name="navs" type="xs:long"/>
					<xs:element name="nom" type="eCH-0044:baseNameType" minOccurs="0"/>
					<xs:element name="prenom" type="eCH-0044:officialFirstNameType" minOccurs="0"/>
					<xs:element name="dateNaissance" type="xs:date" minOccurs="0"/>
					<xs:element name="dateDeces" type="xs:date" minOccurs="0"/>
					<xs:element name="nationalite" type="trax:LandNrBSV_weakType" minOccurs="0"/>
					<xs:element name="domicile" type="trax:WohnkantonStaatBSV_weakType" minOccurs="0"/>
					<xs:element name="refugie" type="xs:boolean"/>
					<xs:element name="renteOrdinaire9" type="rentes_in_host:renteOrdinaire9Type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="renteExtraordinaire9" type="rentes_in_host:renteExtraordinaire9Type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="renteOrdinaire10" type="rentes_in_host:renteOrdinaire10Type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="renteExtraordinaire10" type="rentes_in_host:renteExtraordinaire10Type" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="donneesPostales" type="rentes_in_host:donneesPostalesType" minOccurs="0"/>
					<xs:element name="donneesEuro" type="rentes_in_host:donneesEuroType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="assureType">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:abstractAssureType">
				<xs:sequence>
					<xs:element name="sexe" type="rentes_in_host:sexType"/>
					<xs:element name="dateRemariage" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Ce champ est réservé à l'utilisation de V3 il va disparaître en mode V4, il ne faut pas l'utiliser. Si cet assuré est remarié il faut indiquer l'assuré dans les assurés et les liens dans les familles.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">@io.swagger.v3.oas.annotations.media.Schema(description = "Ce champ est réservé à l'utilisation de V3 il va disparaître en mode V4, il ne faut pas l'utiliser. Si cet assuré est remarié il faut indiquer l'assuré dans les assurés et les liens dans les familles.", required = false)</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="dateRedivorce" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Ce champ est réservé à l'utilisation de V3 il va disparaître en mode V4, il ne faut pas l'utiliser. Si cet assuré est remarié il faut indiquer l'assuré dans les assurés et les liens dans les familles.
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="field">@io.swagger.v3.oas.annotations.media.Schema(description = "Ce champ est réservé à l'utilisation de V3 il va disparaître en mode V4, il ne faut pas l'utiliser. Si cet assuré est remarié il faut indiquer l'assuré dans les assurés et les liens dans les familles.", required = false)</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="donneesAI" type="rentes_in_host:aiInformations" minOccurs="0"/>
					<xs:element name="reductionFauteGrave" type="trax:KuerzungSelbstverschulden_Type" minOccurs="0"/>
					<xs:element name="donneesVeto" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="vetoPrestation">
									<xs:annotation>
										<xs:documentation>1 = transfert, 2 =remboursement, 3 = autre raison
                                        </xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:int">
											<xs:enumeration value="1"/>
											<xs:enumeration value="2"/>
											<xs:enumeration value="3"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="dateVeto" type="xs:date" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="dateDepotRemboursementTransfert" type="xs:date" minOccurs="0"/>
					<xs:element name="flexibilisation" type="rentes_in_host:flexibilisationType" minOccurs="0"/>
					<xs:element name="ci" type="rentes_in_host:ciType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="periodeIJ" type="rentes_in_host:periodeIJType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="periodeTravail" type="rentes_in_host:periodeTravailType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="periodeEtrangere" type="rentes_in_host:periodeEtrangereType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="periode" type="rentes_in_host:periodeType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="chargeEnfant" type="xs:long" minOccurs="0"/>
					<xs:element name="generateur" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
                                Lorsque les deux assurés divorcés sont décédés et que le demandeur est un enfant, ce
                                flag permet de déterminer le décès que l'on veut traiter en priorité (en demande de
                                survivant)
                                Dans tous les autres cas ce flag est ignoré
                            </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="arc" type="rentes_in_host:arcType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="montantRenteEtrangere" type="xs:int" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="arcType">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:Meta">
				<xs:sequence>
					<xs:element name="codeAnnulation">
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:enumeration value="0"/>
								<xs:enumeration value="1"/>
								<xs:enumeration value="2"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="dateValidite" type="xs:gYearMonth"/>
					<xs:element name="motif">
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="1"/>
								<xs:maxInclusive value="99"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="navsConjoint" type="xs:long"/>
					<xs:element name="caisseAgence" type="trax:IrgendeineZweigstelle_weakType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="donneesPostalesType">
		<xs:sequence>
			<xs:element name="adresse" type="rentes_in_host:adresseType"/>
			<xs:element name="banque" type="rentes_in_host:banqueAdresseType"/>
			<xs:element name="mandataire" type="rentes_in_host:organisationAdresseType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="donneesEuroType">
		<xs:sequence>
			<xs:element name="organismeLiaisons" type="rentes_in_host:organismeLiaisonType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="e202" type="rentes_in_host:e202Type" minOccurs="0"/>
			<xs:element name="e203" type="rentes_in_host:e203Type" minOccurs="0"/>
			<xs:element name="e205" type="rentes_in_host:e205Type" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="adresseType">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:Meta">
				<xs:sequence>
					<xs:element name="adresse" type="xs:string"/>
					<xs:element name="localite" type="eCH-0010:localityType"/>
					<xs:element name="codePostal" type="xs:string"/>
					<xs:element name="pays" type="trax:LandNrBSV_weakType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="organisationAdresseType">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:adresseType">
				<xs:sequence>
					<xs:element name="nom" type="xs:string"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="banqueAdresseType">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:organisationAdresseType">
				<xs:sequence>
					<xs:element name="nomTitulaire" type="xs:string"/>
					<xs:element name="bic" type="rentes_in_host:bicType"/>
					<xs:element name="iban" type="rentes_in_host:ibanType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="ibanType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Z]{2,2}[0-9]{2,2}[a-zA-Z0-9]{1,30}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="bicType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Z]{6,6}[A-Z2-9][A-NP-Z0-9]([A-Z0-9]{3,3}){0,1}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="periodeMois">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:Meta">
				<xs:sequence>
					<xs:element name="debut" type="xs:gYearMonth" minOccurs="0"/>
					<xs:element name="fin" type="xs:gYearMonth" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PeriodeJour">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:Meta">
				<xs:sequence>
					<xs:element name="debut" type="xs:date" minOccurs="0"/>
					<xs:element name="fin" type="xs:date" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="periodeIJType">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:PeriodeJour">
				<xs:sequence>
					<xs:element name="mesureNouvelle" type="xs:boolean"/>
					<xs:element name="nbMoisRecouvrement">
						<xs:annotation><xs:documentation>Nombre de mois entier de recouvrement avec une rente AI courant en même temps que la période d'IJ</xs:documentation></xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="3"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="periodeRecueilliGType">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:periodeMois">
				<xs:sequence>
					<xs:element name="tuteur" type="xs:boolean"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="periodeRecueilliCType">
		<xs:complexContent>
			<xs:extension base="rentes_in_host:periodeMois">
				<xs:sequence>
					<xs:element name="parentNonBiologique" type="xs:long"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
