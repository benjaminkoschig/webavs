	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0044/4" schemaLocation="eCH-0044-4-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0010/6" schemaLocation="eCH-0010-6-0.xsd"/>
	<!--<xs:import namespace="http://www.zas.admin.ch/RC"
               schemaLocation="./annoncesRC/Prestations/annoncesRente-types.xsd"/>-->
	<!--<xs:import namespace="http://www.zas.admin.ch/RC"
               schemaLocation="./annoncesRC/rc.xsd"/>-->
	<!--<xs:import namespace="http://www.zas.admin.ch/RC" schemaLocation="./annoncesRC/Prestations/annonceRente.xsd"/>
    <xs:import namespace="http://www.zas.admin.ch/RC" schemaLocation="./annoncesRC/CACI/annonceCI-types.xsd"/>-->
	<xs:import namespace="http://www.zas.admin.ch/RC" schemaLocation="./externalRC.xsd"/>
