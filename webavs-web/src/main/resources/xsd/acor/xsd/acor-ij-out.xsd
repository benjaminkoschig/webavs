<!-- Pour des raisons de compatibilité du xsl qui affiche la feuille ce schema ne peut être avec un targetNamespace (ni avec un namespace). Il faudra voir ci cela est viable-->
<xs:schema  xmlns:arf="http://www.zas.admin.ch/xmlns/arf-out/0"
		    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
			xmlns:annox="http://annox.dev.java.net"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns:inhost="http://www.zas.admin.ch/xmlns/acor-rentes-in-host/0"
            targetNamespace="http://www.zas.admin.ch/xmlns/acor-ij-out/0"
            xmlns="http://www.zas.admin.ch/xmlns/acor-ij-out/0"
            elementFormDefault="qualified"
			attributeFormDefault="unqualified"
			version="1.1"
			jaxb:version="2.1"
			jaxb:extensionBindingPrefixes="annox">
	<xs:import namespace="http://www.zas.admin.ch/xmlns/arf-out/0" schemaLocation="arf-out.xsd"/>
	<xs:import namespace="http://www.zas.admin.ch/xmlns/acor-rentes-in-host/0" schemaLocation="acor-rentes-in-host.xsd"/>
	<xs:complexType name="rente">
		<xs:annotation>
			<xs:documentation>rente en cours</xs:documentation>
			<xs:appinfo>
				<annox:annotate target="class">@io.swagger.v3.oas.annotations.media.Schema(name="RenteCouranteIJ",description = "rente en cours et en historique lors du calcul des IJ")</annox:annotate>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="droit" minOccurs="0">
				<xs:annotation>
					<xs:documentation>législation de base de la rente</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="9"/>
						<xs:enumeration value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="genre" type="GenrePrestation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>* annonce CdC / 
Meldung ZAS
genre de prestation
Leistungsart</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bases" type="id_acor" minOccurs="0">
				<xs:annotation>
					<xs:documentation>id des bases de calcul de la rente, propre à ACOR
Berechnunggrundlagen</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="transfert" type="empty" minOccurs="0">
				<xs:annotation>
					<xs:documentation>issue du transfert dans la législation 10e révision AVS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice minOccurs="0">
			<xs:element name="fraction" type="xs:double">
				<xs:annotation>
					<xs:documentation>fraction de rente AI</xs:documentation>
					<xs:appinfo>
						<annox:annotate target="field">@io.swagger.v3.oas.annotations.media.Schema(name="fraction",description = "fraction de rente (exclusif avec la quotite)",required=false)</annox:annotate>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="quotite">
				<xs:annotation>
					<xs:documentation>quotité de rente AI</xs:documentation>
					<xs:appinfo>
						<annox:annotate target="field">@io.swagger.v3.oas.annotations.media.Schema(name="quotite",description = "quotite de rente après devai (exclusif avec la fraction)",required=false)</annox:annotate>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:double">
						<xs:minInclusive value="0.0"/>
						<xs:maxInclusive value="1.0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			</xs:choice>
			<xs:element name="echelle">
				<xs:annotation>
					<xs:documentation>* annonce CdC /
						Meldung ZAS
						échelle de rente
						Skala. Les rente extrordinaire n'ont pas d'échelle.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="44"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="etat" maxOccurs="unbounded">
				<xs:complexType>
					<xs:annotation><xs:documentation>etat d'une prestation rente dans les IJ</xs:documentation>
						<xs:appinfo>
							<annox:annotate target="class">@io.swagger.v3.oas.annotations.media.Schema(name="EtatRenteIJ",description = "etat d'une prestation rente dans les IJ")</annox:annotate>
						</xs:appinfo>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="an" type="AAAA">
							<xs:annotation>
								<xs:documentation>année d'état du ram et du montant
Jahr des DJE und Betrags</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ram" minOccurs="0">
							<xs:annotation>
								<xs:documentation>revenu annuel moyen
									Durchschnittliches Jahreseinkommen. Les rentes extraordinaires n'ont pas de ram.
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:int">
									<xs:minInclusive value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="montant">
							<xs:annotation>
								<xs:documentation>* annonce CdC /
									Meldung ZAS
									montant effectif de la rente
									Betrag
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:int">
									<xs:minInclusive value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="debut_droit" type="AAAAMMJJ">
				<xs:annotation>
					<xs:documentation>* annonce CdC / 
Meldung ZAS
Début de droit à la rente
Anspruchsbeginn</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fin_droit" type="AAAAMMJJ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>* annonce CdC / 
Meldung ZAS
fin de droit à la rente
Ende des Anspruchs</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="JoursMaladie">
		<xs:sequence>
			<xs:element name="annee">
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="debut" type="AAAAMMJJ"/>
			<xs:element name="max_jour" type="xs:int"/>
			<xs:element name="total" type="xs:int"/>
			<xs:element name="decompte" type="DecompteMaladie" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DecompteMaladie">
		<xs:sequence>
			<xs:element name="debut" type="AAAAMMJJ"/>
			<xs:element name="fin" type="AAAAMMJJ"/>
			<xs:element name="nbr_jours" type="xs:int"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Decompte">
		<xs:sequence>
			<xs:element name="statut">
				<xs:annotation>
					<xs:documentation>1=normal (nouveau décompte)
						2=rectificateur
						3=annulateur (négatif)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="id_ij_cause" type="xs:string">
				<xs:annotation>
					<xs:documentation>Si statut =2 ou 3 et que la cause de la rectification est une rectification d'IJ l'identifiant de l'IJ cocernée, sinon vide</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mois_comptable" type="AAAAMM"/>
			<xs:element name="contenu_annonce">
				<xs:annotation>
					<xs:documentation>0=inconnu
						1=normal (nouveau décompte)
						3=décompte complémentaire
						4=restitution (négatif)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="1"/>
						<xs:enumeration value="3"/>
						<xs:enumeration value="4"/>
						<xs:enumeration value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="date_debut" type="AAAAMMJJ"/>
			<xs:element name="date_fin" type="AAAAMMJJ"/>
			<xs:element name="njours_ext" type="xs:int"/>
			<xs:element name="njours_int" type="xs:int"/>
			<xs:element name="genre_beneficiaire">
				<xs:annotation><xs:documentation>genre du ou des bénéficiaire 1=assure, 2=employeur, 3=mixte (nécessaire pour l'annonce 8G)</xs:documentation></xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="njours_interruption" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="motif_interruption" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="montant_global" type="xs:double"/>
			<xs:element name="type_cotisations">
				<xs:annotation>
					<xs:documentation>1=AVS/AI/APG/AC  2=AVS/AI/APG</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="taux_cotisations" type="xs:double"/>
			<xs:element name="montant_cotisations" type="xs:double"/>
			<xs:sequence minOccurs="0">
				<xs:element name="canton_impots">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="taux_impots">
					<xs:simpleType>
						<xs:restriction base="xs:double">
							<xs:minInclusive value="0"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="montant_impots" type="xs:double"/>
			</xs:sequence>
			<xs:element name="part_patronale" type="xs:double"/>
			<xs:element name="deduction_AO" type="xs:double"/>
			<xs:element name="montant_net" type="xs:double"/>
			<xs:element name="montant_net2" type="xs:double">
				<xs:annotation>
					<xs:documentation>Si le décompte rectifie un autre décompte alors différence entre les deux, sinon montant_net</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="decompte_categorie" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="categorie">
							<xs:annotation>
								<xs:documentation>0=non pertinent  (enfants)
									1=interne
									2=externe</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:int">
									<xs:enumeration value="1"/>
									<xs:enumeration value="2"/>
									<xs:enumeration value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="type">
							<xs:annotation>
								<xs:documentation>1=indemnite principale (adulte)
									2=prestation reduite pour enfant
									3=prestation pour enfant</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:int">
									<xs:enumeration value="1"/>
									<xs:enumeration value="2"/>
									<xs:enumeration value="3"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="n_jours" type="xs:int"/>
						<xs:element name="montant_categorie_j" type="xs:double"/>
						<xs:element name="montant_categorie_total" type="xs:double"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="repartition" minOccurs="0" maxOccurs="2">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="type">
							<xs:annotation>
								<xs:documentation>2=assure
									3=enfants</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:int">
									<xs:enumeration value="2"/>
									<xs:enumeration value="3"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="montant_versement" type="xs:double"/>
						<xs:sequence minOccurs="0">
							<xs:element name="montant_cotisations">
								<xs:simpleType>
									<xs:restriction base="xs:double">
										<xs:maxInclusive value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="taux_cotisations">
								<xs:simpleType>
									<xs:restriction base="xs:double">
										<xs:minInclusive value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
						<xs:element name="deduction_AO" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:double">
									<xs:minInclusive value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="montant_impots" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:double">
									<xs:maxInclusive value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="montant_net" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:double">
									<xs:minInclusive value="0"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="paiement" type="Paiement" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="refus_paiement" type="RefusPaiement" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="remarques" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:annotation><xs:documentation>remarque dans un décompte d'IJ</xs:documentation><xs:appinfo>
						<annox:annotate target="class">@io.swagger.v3.oas.annotations.media.Schema(name="RemarqueDecompteIJ",description = "remarque dans un décompte d'IJ")</annox:annotate>
					</xs:appinfo>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="nom" type="xs:string"/>
						<xs:element name="label" type="xs:string"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="historique" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>true = le décompte est reprisd'un ancien système il ne doit pas faire l'objet d'une annonce ou d'une comptabilisation
						false = décompte normal</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DecompteIJ">
		<xs:complexContent>
			<xs:extension base="Decompte">
				<xs:sequence>
					<xs:element name="jours_maladie" type="JoursMaladie" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="origine">
							<xs:annotation><xs:documentation>origine du décompte : GUI = saisie dans le GUI par le gestionnaire, CLIPS = créé dans l'analyse (par exemple pour les décomptes scindés en deux), DB = était déjà en entrée (cela n'arrive que pour un décompte déplacé sans modification d'une base de calcul à une autre)</xs:documentation></xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="GUI"/>
								<xs:enumeration value="CLIPS"/>
								<xs:enumeration value="DB"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Paiement">
		<xs:sequence>
			<xs:element name="montant" type="xs:double"/>
			<xs:element name="beneficiaire">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="denomination" type="xs:string"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RefusPaiement">
		<xs:sequence>
			<xs:element name="montant" type="xs:string">
				<xs:annotation>
					<xs:documentation>Soit "Solde" soit le montant</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="denomination" type="xs:string"/>
			<xs:element name="raison" type="xs:string"/>
			<xs:element name="label" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="empty">
		<xs:sequence/>
	</xs:complexType>
	<xs:element name="f_calcul">
		<xs:complexType mixed="0">
			<xs:annotation>
				<xs:documentation>
				La feuille de calcul (format de sortie) pour les IJ
				</xs:documentation>
				<xs:appinfo>
					<annox:annotate target="class">@io.swagger.v3.oas.annotations.media.Schema(name="FCalculIJ",description = "La feuille de calcul (format de sortie) pour les IJ")</annox:annotate>
				</xs:appinfo>
			</xs:annotation>
			<xs:sequence>
				<xs:element name="version_expert" type="xs:string"/>
				<xs:element name="demande">
					<xs:complexType>
						<xs:annotation>
							<xs:documentation>
								La demande (out) pour les IJ
							</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="class">@io.swagger.v3.oas.annotations.media.Schema(name="DemandeIJ",description = "La demande (out) pour les IJ ")</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
						<xs:sequence>
							<xs:element name="id" type="xs:string">
								<xs:annotation>
									<xs:documentation>id de la demande</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="type" type="xs:string" minOccurs="0"/>
							<xs:element name="langue" type="xs:string" minOccurs="0"/>
							<xs:element name="caisse" type="xs:int" minOccurs="0"/>
							<xs:element name="date_depot" type="AAAAMMJJ" minOccurs="0"/>
							<xs:element name="date_traitement" type="AAAAMMJJ"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="assure" maxOccurs="unbounded">
					<xs:complexType>
						<xs:annotation>
							<xs:documentation>Assuré: individu de la famille des assurés pris en compte dans le dossier IJ Versicherte(r)</xs:documentation>
							<xs:appinfo>
								<annox:annotate target="class">@io.swagger.v3.oas.annotations.media.Schema(name="AssureIJ",description = "Assuré: individu de la famille des assurés pris en compte dans le dossier IJ Versicherte(r)")</annox:annotate>
							</xs:appinfo>
						</xs:annotation>
						<xs:sequence>
							<xs:element name="id">
								<xs:complexType>
									<xs:annotation>
										<xs:documentation>id de l'assure IJ VersicherterId</xs:documentation>
										<xs:appinfo>
											<annox:annotate target="class">@io.swagger.v3.oas.annotations.media.Schema(name="AssureIdIJ",description = "id de l'assure IJ VersicherterId")</annox:annotate>
										</xs:appinfo>
									</xs:annotation>
									<xs:simpleContent>
										<xs:extension base="id_acor">
											<xs:attribute name="type">
												<xs:annotation>
													<xs:documentation>navs, nnss ou autre</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="navs"/>
														<xs:enumeration value="navs11"/>
														<xs:enumeration value="autre"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="nom" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>nom de l'assure
Name der Versicherter</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="fonction" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="req"/>
										<xs:enumeration value="conj"/>
										<xs:enumeration value="enf"/>
										<xs:enumeration value="ex_epouse"/>
										<xs:enumeration value="ex_epoux"/>
										<xs:enumeration value="mere_nat"/>
										<xs:enumeration value="pere_nat"/>
										<xs:enumeration value="enf_nat"/>
										<xs:enumeration value="enf_ex"/>
										<xs:enumeration value="ex_conjoint"/>
										<xs:enumeration value="parent"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="sexe" minOccurs="0">
								<xs:annotation>
									<xs:documentation>f=femme / Frau 
h=homme / Mann</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
