					<xsd:documentation xml:lang="en">personal address</xsd:documentation>
					<xsd:documentation xml:lang="de"/>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="e205:Birth" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">field 4</xsd:documentation>
					<xsd:documentation xml:lang="en">information of the birth</xsd:documentation>
