	<xs:annotation>
		<xs:documentation xml:lang="de">Basistypen für die Meldungen SWAP 2401.</xs:documentation>
		<xs:documentation xml:lang="fr">Types de base pour les messages SWAP 2401.</xs:documentation>
		<xs:documentation xml:lang="it">Tipi di base per i messaggi SWAP 2401.</xs:documentation>
	</xs:annotation>

    <xs:import namespace="http://www.ech.ch/xmlns/eCH-0010-f/6" schemaLocation="eCH-0010-6-0f.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0044-f/4" schemaLocation="eCH-0044-4-1f.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0046-f/3" schemaLocation="eCH-0046-3-0f.xsd"/>
    <xs:import namespace="http://www.eahv-iv.ch/xmlns/eahv-iv-common/4" schemaLocation="eahv-iv-common-4-0.xsd"/>


	<!-- SWAP header specifics -->

	<xs:simpleType name="documentTypeType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Dokumenttyp, fachliche Beschreibung des Dokuments.
			</xs:documentation>
			<xs:documentation xml:lang="fr">Type de document, description technique du document.
			</xs:documentation>
			<xs:documentation xml:lang="it">Tipo di documento, descrizione tecnica del documento.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(01)(\.[0-9][0-9])*" />
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="attachmentType">
		<xs:sequence>
			<xs:element name="title" type="eahv-iv-common:attachmentTitleType" />
			<xs:element name="documentDate" type="eahv-iv-common:documentDateType" />
			<xs:element name="leadingDocument" type="eahv-iv-common:leadingDocumentType" />
			<xs:element name="sortOrder" type="eahv-iv-common:sortOrderType" />
			<xs:element name="documentFormat">
				<xs:simpleType>
					<xs:restriction base="eahv-iv-common:documentFormatType">
						<xs:enumeration value="application/pdf" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="documentType" type="eahv-iv-common:documentKindType" />
			<xs:element name="file" type="eahv-iv-common:attachmentFileType" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="extensionType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Attribute, welche im eCH-0058/4-Standard nicht vorgesehen sind,
				werden im Attribut "extension" geführt.
			</xs:documentation>
			<xs:documentation xml:lang="fr">Les attributs qui ne sont pas prévus dans eCH-0058/4-Standard
				doivent être effectués dans l'attribut
				"extension".
			</xs:documentation>
			<xs:documentation xml:lang="it">Gli attributi che non sono previsti nel eCH-0058/4-Standard
				vengono effettuati nel attributo
				"extension".
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="contactInformation" type="eahv-iv-common:contactInformationType" />
		</xs:sequence>
	</xs:complexType>

	<!-- SWAP content specifics -->

	<xs:complexType name="addressInformationType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Der Standard addressInformationType aus eCH-0010 ist zu
				umfangreich und wird daher nur partiell
				verwendet
			</xs:documentation>
			<xs:documentation xml:lang="fr">Le standard addressInformationType de eCH-0010 est trop vaste et
				n'est donc que partiellement utilisé
			</xs:documentation>
			<xs:documentation xml:lang="it">Il standard addressInformationType da eCH-0010 e troppo vasto ed
				e quindi utilizzato solo parzialmente
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="streetWithNr" type="eai-2401-common:baseNameMax155Type">
				<xs:annotation>
					<xs:documentation xml:lang="de">Strasse und Nummer</xs:documentation>
					<xs:documentation xml:lang="fr">Rue et numéro</xs:documentation>
					<xs:documentation xml:lang="it">Via e numero</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zipCode" type="eCH-0010:foreignZipCodeType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Postleitzahl</xs:documentation>
					<xs:documentation xml:lang="fr">Code postal</xs:documentation>
					<xs:documentation xml:lang="it">Codice postale</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="town" type="eCH-0010:townType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Stadt</xs:documentation>
					<xs:documentation xml:lang="fr">Ville</xs:documentation>
					<xs:documentation xml:lang="it">Città</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="region" type="eai-2401-common:baseNameMax65Type" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="de">Region</xs:documentation>
					<xs:documentation xml:lang="fr">Région</xs:documentation>
					<xs:documentation xml:lang="it" />
				</xs:annotation>
			</xs:element>
			<xs:element name="country" type="eCH-0010:countryIdISO2Type">
				<xs:annotation>
					<xs:documentation xml:lang="de">Land ISO2</xs:documentation>
					<xs:documentation xml:lang="fr">Pays ISO2</xs:documentation>
					<xs:documentation xml:lang="it">Paese ISO2</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
