					<xs:documentation xml:lang="fr" />
					<xs:documentation xml:lang="it" />
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="quart">
				<xs:annotation>
					<xs:documentation xml:lang="en">quarterly</xs:documentation>
