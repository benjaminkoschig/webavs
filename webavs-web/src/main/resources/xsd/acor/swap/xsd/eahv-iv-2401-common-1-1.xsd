					<xs:documentation xml:lang="fr" />
					<xs:documentation xml:lang="it" />
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="int_ret_se">
				<xs:annotation>
					<xs:documentation xml:lang="en">Person intends to retire from self-employment
					</xs:documentation>
					<xs:documentation xml:lang="de" />
					<xs:documentation xml:lang="fr" />
