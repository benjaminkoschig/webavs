					<xs:documentation xml:lang="fr" />
					<xs:documentation xml:lang="it" />
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mon12">
				<xs:annotation>
					<xs:documentation xml:lang="en">monthly (12 months)</xs:documentation>
