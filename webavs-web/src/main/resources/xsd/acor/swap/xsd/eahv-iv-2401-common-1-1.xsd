			<xs:element name="benefitAmount" type="eai-2401-common:generalAmountType" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="sicknessBenefitEntitlement" type="eai-2401-common:enumNoYesNotdetermined" minOccurs="0" maxOccurs="1" />
		</xs:sequence>
