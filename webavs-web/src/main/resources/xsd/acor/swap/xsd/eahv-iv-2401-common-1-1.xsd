			<xs:element name="amountEffectivSince" type="xs:date" minOccurs="0" maxOccurs="1" />
			<xs:element name="paymentFrequency" type="eai-2401-common:enumPaymentFrequencyType" />
			<xs:element name="paymentFrequencyOther" type="eai-2401-common:baseNameMax255Type" minOccurs="0" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="enumPaymentFrequencyType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="5" />
