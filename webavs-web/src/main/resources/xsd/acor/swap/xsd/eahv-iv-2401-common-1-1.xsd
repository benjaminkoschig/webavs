	<xs:simpleType name="enumBenefitStatusType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="5" />
			<xs:enumeration value="apl">
				<xs:annotation>
					<xs:documentation xml:lang="en">Applied</xs:documentation>
					<xs:documentation xml:lang="de">Beantragt</xs:documentation>
					<xs:documentation xml:lang="fr">Applique</xs:documentation>
					<xs:documentation xml:lang="it" />
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="grn">
				<xs:annotation>
					<xs:documentation xml:lang="en">Granted</xs:documentation>
					<xs:documentation xml:lang="de">Erteilt</xs:documentation>
					<xs:documentation xml:lang="fr">Accorde</xs:documentation>
