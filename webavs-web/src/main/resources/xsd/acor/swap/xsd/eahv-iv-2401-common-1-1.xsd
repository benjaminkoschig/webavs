			</xs:enumeration>
			<xs:enumeration value="mon14">
				<xs:annotation>
					<xs:documentation xml:lang="en">monthly (14 months)</xs:documentation>
					<xs:documentation xml:lang="de">monatlich (14 Monate)</xs:documentation>
					<xs:documentation xml:lang="fr" />
					<xs:documentation xml:lang="it" />
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="week">
				<xs:annotation>
					<xs:documentation xml:lang="en">weekly</xs:documentation>
					<xs:documentation xml:lang="de">wöchentlich</xs:documentation>
					<xs:documentation xml:lang="fr" />
					<xs:documentation xml:lang="it" />
				</xs:annotation>
