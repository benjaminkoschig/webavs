				<xs:element name="importModus" type="eahv-iv-2401-common:enumImportModusType"/>
				<xs:element name="pensionData" type="eahv-iv-2401-common:pensionDataType" nillable="false" minOccurs="1" maxOccurs="1"/>
			</xs:sequence>
			<xs:attribute name="minorVersion" type="xs:int" use="required"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
