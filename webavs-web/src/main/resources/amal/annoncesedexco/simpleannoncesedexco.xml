<?xml version="1.0" encoding="UTF-8"?>
<model-mapping>
	<simple-model className="ch.globaz.amal.business.models.annoncesedexco.SimpleAnnonceSedexCO">
		<description>Enregistrement Annonce Sedex contentieux</description>
		<table name="MASDXCO" hasSpy="true" hasCreationSpy="true" alwaysIncrementPrimaryKey="true"/>
		<fields>
			<primary-key name="idAnnonceSedexCO" column="ID" comment="clé primaire"/>
			<field name="messageId" column="MSGID" type="STRING" dataType="VARCHAR(40)" mandatory="false" comment="Message ID"/>
			<field name="businessProcessId" column="BPRID" type="STRING" dataType="VARCHAR(40)" mandatory="false" comment="Business process id"/>			
			<field name="messageType" column="MESSAGE_TYPE" type="NUMERIC" dataType="NUMERIC(8,0)" mandatory="false" comment="TYPE_MESSAGE_SEDEX_RIP"/>
			<field name="messageSubType" column="MESSAGE_SUBTYPE" type="NUMERIC" dataType="NUMERIC(8,0)" mandatory="false" comment="SUBTYPE_MESSAGE_SEDEX"/>
			<field name="messageEmetteur" column="EMETTEUR" type="STRING" dataType="VARCHAR(20)" mandatory="false" comment="ID_SEDEX_EMETTEUR"/>
			<field name="messageRecepteur" column="RECEPTEUR" type="STRING" dataType="VARCHAR(20)" mandatory="false" comment="ID_SEDEX_RECEPTEUR"/>
			<field name="idContribuable" column="IDCONT" type="NUMERIC" dataType="NUMERIC(15,0)" mandatory="true" comment="ID_CONTRIBUABLE_DOSSIER"/>
			<field name="idFamille" column="IDFAMI" type="NUMERIC" dataType="NUMERIC(15,0)" mandatory="true" comment="ID_MEMBRE_FAMILLE"/>
			<field name="dateAnnonce" column="DATE_ANNONCE" type="DATE" mandatory="false" comment="DATE_ANNONCE"/>
			<field name="periodeDebut" column="PERIODE_DEBUT" type="DATE" mandatory="false" comment="PERIODE_DEBUT"/>
			<field name="periodeFin" column="PERIODE_FIN" type="DATE" mandatory="false" comment="PERIODE_FIN"/>
			<field name="interets" column="INTERETS" type="NUMERIC" dataType="NUMERIC(10,0)" mandatory="false" comment="Intérêts"/>
			<field name="frais" column="FRAIS" type="NUMERIC" dataType="NUMERIC(10,0)" mandatory="false" comment="Frais"/>
			<field name="totalCreance" column="TOTAL_CREANCE" type="NUMERIC" dataType="NUMERIC(10,0)" mandatory="false" comment="Total créance"/>
			<field name="pmtDebiteur" column="PMT_DEBITEUR" type="NUMERIC" dataType="NUMERIC(10,0)" mandatory="false" comment="Paiement débiteur"/>
			<field name="rpRetro" column="RP_RETRO" type="NUMERIC" dataType="NUMERIC(10,0)" mandatory="false" comment="RP rétro"/>
			<field name="annulation" column="ANNULATION" type="NUMERIC" dataType="NUMERIC(10,0)" mandatory="false" comment="Annulation"/>
		</fields>
				
		<order-by>
			<order-field field-ref="idAnnonceSedexCO" order="DESC"/>
			<order-field field-ref="dateAnnonce" order="DESC"/>
		</order-by>
		
	</simple-model>
</model-mapping>