			<field name="montantDebloque" type="NUMERIC" dataType="NUMERIC(15,2)" column="ZZMDBK" comment="MONTANT_DEBLOQUE" />
		</fields>
		<search-definition>
			<search-group operator="AND">
				<search-field name="forCsType" field-ref="csTypeLot" operation="EQUALS"/>
