			<field name="dateDebut" type="DATE" column="WKDDEB" comment="DATE_DEBUT"/>
			<field name="dateFin" type="DATE" column="WKDFIN" comment="DATE_FIN"/>
			<field name="typeRelation" type="NUMERIC" column="WKTTYP" comment="TYPE_RELATION"/>
